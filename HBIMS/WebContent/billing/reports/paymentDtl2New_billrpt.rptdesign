            <method name="beforeOpen"><![CDATA[this.queryText="SELECT DECODE(NVL(A.HRGNUM_PUK,0),0,DECODE(NVL(A.HBLSTR_PREV_PUK,'0'),'0','-',A.HBLSTR_PREV_PUK),A.HRGNUM_PUK) AS cr_No, INITCAP(DECODE(NVL(A.HRGNUM_PUK,0),0,A.HBLSTR_PATIENT_NAME,Ahis_Function.fun_pat_name(A.HRGNUM_PUK))) AS pat_Name,TO_CHAR(A.HBLDT_RECIEPT_DATE,'DD-Mon-YYYY') AS date_val,(A.HBLNUM_BILL_NO || '/' || A.HBLNUM_RECIEPT_NO) AS bill_No, NVL(B.HBLNUM_RECIEPT_AMT,0) AS bill_Amt, Bill_Mst.getPaymentModeName(HBLNUM_PAYMENT_MODE,A.GNUM_HOSPITAL_CODE) AS payment_Mode, (Bill_Mst.get_receipt_name(A.HBLNUM_RECIEPT_TYPE) || '/' ||Bill_Mst.getbservicename(A.SBLNUM_BSERVICE_ID,0,A.GNUM_HOSPITAL_CODE)) AS  payment_Against, HBLSTR_PAYMENT_DTLS AS payment_dtls  FROM HBLT_BILL_RECIEPT_DTL A, HBLT_PAYMENT_DETAIL B WHERE A.GNUM_ISVALID = 1 AND B.GNUM_ISVALID = 1 AND A.HBLNUM_STATUS = 1 AND B.HBLNUM_PAYMENT_STATUS IN (0,1) AND A.GNUM_HOSPITAL_CODE = B.GNUM_HOSPITAL_CODE AND A.HBLNUM_RECIEPT_NO = B.HBLNUM_RECIEPT_NO AND A.HBLNUM_BILL_NO = B.HBLNUM_BILL_NO ";
this.queryText = this.queryText + "AND A.GNUM_HOSPITAL_CODE = "+  "'" +params["hosp_Code"] + "'" ;

if(parseInt(params["charge_Type_Id"].value)>0)
{
this.queryText=this.queryText+"  AND A.SBLNUM_CHARGETYPE_ID = "+"'"+params["charge_Type_Id"]+"'";
}


if(parseInt(params["payment_mode_Id"].value)>0)
{
this.queryText=this.queryText+"  AND 	B.HBLNUM_PAYMENT_MODE = "+"'"+params["payment_mode_Id"]+"'";
}


this.queryText=this.queryText+" AND A.HBLDT_RECIEPT_DATE >= to_date(to_char("  +  "'" +params["from_Date"]+"'::timestamp,'dd-mm-yyyy'),'dd-mm-yyyy')";

this.queryText=this.queryText+"AND A.HBLDT_RECIEPT_DATE <=  to_date(to_char("  +  "'" +params["to_Date"]+ "'::timestamp,'dd-mm-yyyy'),'dd-mm-yyyy')" ;
var patName=params["pat_Name"];

this.quertText=this.queryText+" AND UPPER(DECODE(NVL(A.HRGNUM_PUK,0),0,HBLSTR_PATIENT_NAME,Ahis_Function.fun_pat_name(A.HRGNUM_PUK))) LIKE UPPER('"+patName+"%')"  ;  
this.queryText=this.queryText+ " ORDER BY A.HBLDT_RECIEPT_DATE,A.HRGNUM_PUK";]]></method>
