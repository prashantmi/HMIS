            <method name="beforeOpen"><![CDATA[this.queryText= "(SELECT Bill_Mst.getCounterDtl(0,'',A.GNUM_HOSPITAL_CODE,2,A.HBLNUM_COUNTER_ID)AS counter_Name,(SELECT gstr_usr_name FROM GBLT_USER_MST WHERE gnum_userid = A.GNUM_SEATID AND gnum_isvalid = 1) AS clerk_Name,TO_CHAR(HBLDT_RECIEPT_DATE,'DD-Mon-YYYY') AS date_data,(A.HBLNUM_BILL_NO || '/' || A.HBLNUM_RECIEPT_NO) AS bill_No,DECODE(NVL(A.HRGNUM_PUK,0),0,DECODE(NVL(A.HBLSTR_PREV_PUK,'0'),'0','-',A.HBLSTR_PREV_PUK),A.HRGNUM_PUK) || ' '::varchar2 AS cr_No,INITCAP(DECODE(NVL(A.HRGNUM_PUK,0),0,A.HBLSTR_PATIENT_NAME,Ahis_Function.fun_pat_name(A.HRGNUM_PUK)))AS pat_Name,(Bill_Mst.get_receipt_name(A.HBLNUM_RECIEPT_TYPE) || '/'||Bill_Mst.getbservicename(A.SBLNUM_BSERVICE_ID,0,a.GNUM_HOSPITAL_CODE,a.SBLNUM_CHARGETYPE_ID)) AS  payment_Against, DECODE(A.HBLNUM_PATIENT_TOT_AMT,0,DECODE(A.HBLNUM_CLIENT_TOT_AMT,0,A.HBLNUM_PATIENT_TOT_AMT,HBLNUM_CLIENT_TOT_AMT),(A.HBLNUM_PATIENT_TOT_AMT+A.HBLNUM_CLIENT_TOT_AMT)) AS rece_Amt,DECODE(NVL(a.hastr_adm_no,0) , 0 , '' ,    a.hastr_adm_no ) ::character varying AS  adm_no FROM HBLT_BILL_RECIEPT_DTL A WHERE A.GNUM_ISVALID = 1 AND A.HBLNUM_STATUS = 1";

this.queryText = this.queryText + " AND BILL_MST.GET_BILL_PAYMENTMODE(A.HRGNUM_PUK,A.HBLNUM_BILL_NO,A.HBLNUM_RECIEPT_NO,A.GNUM_HOSPITAL_CODE)<>DECODE(A.SBLNUM_BSERVICE_ID,19,10,20,10,0)  AND A.GNUM_HOSPITAL_CODE IN ( " +params["hosp_Code"] + ")" ;

this.queryText=this.queryText+" AND trunc(A.HBLDT_RECIEPT_DATE) >= to_date(to_char("  +  "'" +params["from_Date"]+"'::timestamp,'dd-mm-yyyy'),'dd-mm-yyyy')";

this.queryText=this.queryText+"AND trunc(A.HBLDT_RECIEPT_DATE) <=  to_date(to_char("  +  "'" +params["to_Date"]+ "'::timestamp,'dd-mm-yyyy'),'dd-mm-yyyy')" ;

if(parseInt(params["charge_Type_Id"].value) > 0){
               this.queryText = this.queryText + " AND A.SBLNUM_CHARGETYPE_ID = "+  "'"  + params["charge_Type_Id"] +"'" ;
          }
          
if(parseInt(params["counter_Id"].value) > 0){
              this.queryText = this.queryText + "  AND A.HBLNUM_COUNTER_ID = "+  "'" + params["counter_Id"]+ "'" ;
}

this.queryText = this.queryText +" ORDER BY A.HBLDT_RECIEPT_DATE,A.HBLNUM_BILL_NO,A.HBLNUM_RECIEPT_NO)";

this.queryText= this.queryText+" UNION ";

this.queryText= this.queryText+" (SELECT BILL_MST.GETCOUNTERDTL(0,'',B.GNUM_HOSPITAL_CODE,2,B.GNUM_COUNTER_ID)AS counter_Name,(SELECT gstr_usr_name FROM GBLT_USER_MST WHERE gnum_userid = B.GNUM_SEAT_ID AND gnum_isvalid = 1) AS clerk_Name,TO_CHAR(GDT_ENTRY_DATE,'DD-Mon-YYYY') AS date_data,(HBLNUM_BILL_NO || '/' || HBLNUM_RECIEPT_NO) AS bill_No,(NVL(B.HRGNUM_PUK,0)::varchar2)AS cr_No,INITCAP(DECODE(NVL(HRGNUM_PUK,0),0,'',AHIS_FUNCTION.FUN_PAT_NAME(HRGNUM_PUK)))AS pat_Name,(BILL_MST.GET_RECEIPT_NAME(HBLNUM_RECIEPT_TYPE) || '/'||BILL_MST.GETBSERVICENAME(SBLNUM_BSERVICE_ID,0,B.GNUM_HOSPITAL_CODE,B.SBLNUM_CHARGETYPE_ID))||'('|| bill_mst.gettariffname(B.hblstr_tariff_id::numeric, 0::numeric, B.GNUM_HOSPITAL_CODE::numeric) ||')' AS  payment_Against, NVL(HRGNUM_AMT_COLLECTED,0) AS rece_Amt,DECODE(NVL(b.hrgnum_admission_no,0) , 0 , '' ,   b.hrgnum_admission_no ) ::character varying AS  adm_no FROM HBLT_DIRECT_CHARGE_DTL B WHERE GNUM_ISVALID = 1";

this.queryText = this.queryText + "AND GNUM_HOSPITAL_CODE IN ( " +params["hosp_Code"] + ")" ;

this.queryText=this.queryText+" AND trunc(GDT_ENTRY_DATE) >= to_date(to_char("  +  "'" +params["from_Date"]+"'::timestamp,'dd-mm-yyyy'),'dd-mm-yyyy')";

this.queryText=this.queryText+"AND trunc(GDT_ENTRY_DATE) <=  to_date(to_char("  +  "'" +params["to_Date"]+ "'::timestamp,'dd-mm-yyyy'),'dd-mm-yyyy')" ;

if(parseInt(params["charge_Type_Id"].value) > 0){
               this.queryText = this.queryText + " AND SBLNUM_CHARGETYPE_ID = "+  "'"  + params["charge_Type_Id"] +"'" ;
          }

if(parseInt(params["counter_Id"].value) > 0){
              this.queryText = this.queryText + "  AND B.GNUM_COUNTER_ID = "+  "'" + params["counter_Id"]+ "'" ;
 }
 
 this.queryText = this.queryText +" ORDER BY trunc(GDT_ENTRY_DATE),HBLNUM_BILL_NO,HBLNUM_RECIEPT_NO)";
 
 this.queryText= this.queryText+" UNION ";

this.queryText= this.queryText+" (SELECT BILL_MST.GETCOUNTERDTL(0,'',C.GNUM_HOSPITAL_CODE,2,C.GNUM_COUNTER_ID)AS counter_Name,(SELECT gstr_usr_name FROM GBLT_USER_MST WHERE gnum_userid = C.GNUM_SEAT_ID AND gnum_isvalid = 1) AS clerk_Name,TO_CHAR(GDT_ENTRY_DATE,'DD-Mon-YYYY') AS date_data,(HBLNUM_BILL_NO || '/' || HBLNUM_RECIEPT_NO) AS bill_No,(NVL(C.HRGNUM_PUK,0)::varchar2)AS cr_No,INITCAP(DECODE(NVL(HRGNUM_PUK,0),0,'',AHIS_FUNCTION.FUN_PAT_NAME(HRGNUM_PUK)))AS pat_Name,(BILL_MST.GET_RECEIPT_NAME(HBLNUM_RECIEPT_TYPE) || '/'||BILL_MST.GETBSERVICENAME(SBLNUM_BSERVICE_ID,0,C.GNUM_HOSPITAL_CODE,C.SBLNUM_CHARGETYPE_ID))||'('|| bill_mst.gettariffname(C.hblstr_tariff_id::numeric, 0::numeric, C.GNUM_HOSPITAL_CODE::numeric) ||')' AS  payment_Against, NVL(HRGNUM_AMT_COLLECTED,0) AS rece_Amt,DECODE(NVL(c.hrgnum_admission_no,0) , 0 , '' ,   c.hrgnum_admission_no ) ::character varying AS  adm_no FROM HBLT_DIRECT_CHARGE_HIST_DTL C WHERE GNUM_ISVALID = 1";

this.queryText = this.queryText + "AND GNUM_HOSPITAL_CODE IN ( " +params["hosp_Code"] + ")" ;

this.queryText=this.queryText+" AND trunc(GDT_ENTRY_DATE) >= to_date(to_char("  +  "'" +params["from_Date"]+"'::timestamp,'dd-mm-yyyy'),'dd-mm-yyyy')";

this.queryText=this.queryText+"AND trunc(GDT_ENTRY_DATE) <=  to_date(to_char("  +  "'" +params["to_Date"]+ "'::timestamp,'dd-mm-yyyy'),'dd-mm-yyyy')" ;

if(parseInt(params["charge_Type_Id"].value) > 0){
               this.queryText = this.queryText + " AND SBLNUM_CHARGETYPE_ID = "+  "'"  + params["charge_Type_Id"] +"'" ;
          }
if(parseInt(params["counter_Id"].value) > 0){
              this.queryText = this.queryText + "  AND C.GNUM_COUNTER_ID = "+  "'" + params["counter_Id"]+ "'" ;
 }
 
this.queryText = this.queryText +" ORDER BY trunc(GDT_ENTRY_DATE),HBLNUM_BILL_NO,HBLNUM_RECIEPT_NO)";

this.queryText = this.queryText +" ORDER BY date_data,bill_No";]]></method>
