            <xml-property name="queryText"><![CDATA[SELECT Bill_Mst.getCounterDtl(0,'',A.GNUM_HOSPITAL_CODE,2,A.HBLNUM_COUNTER_ID)
AS counter_Name,
                    ''
AS clerk_Name,
                    TO_CHAR(HBLDT_RECIEPT_DATE,'DD-Mon-YYYY') AS date_data,
                    (HBLNUM_BILL_NO || '/' || HBLNUM_RECIEPT_NO) AS bill_No,
                    DECODE(NVL(A.HRGNUM_PUK,0),0,DECODE(NVL(HBLSTR_PREV_PUK,'0'),'0','-',HBLSTR_PREV_PUK),HRGNUM_PUK)||' '
::varchar2 AS cr_No,
                    INITCAP(DECODE(NVL(HRGNUM_PUK,0),0,HBLSTR_PATIENT_NAME,Ahis_Function.fun_pat_name(HRGNUM_PUK)))
AS pat_Name,
                    (Bill_Mst.get_receipt_name(HBLNUM_RECIEPT_TYPE) || '/' ||
                    Bill_Mst.getbservicename(SBLNUM_BSERVICE_ID,0,a.GNUM_HOSPITAL_CODE))
AS  payment_Against,
                    NVL(HBLNUM_PATIENT_TOT_AMT,0) AS rece_Amt,DECODE(NVL(a.hastr_adm_no,0) , 0 , '' ,    a.hastr_adm_no ) ::character varying AS  adm_no
                    FROM HBLT_BILL_RECIEPT_DTL A
UNION ALL
SELECT BILL_MST.GETCOUNTERDTL(0,'',B.GNUM_HOSPITAL_CODE,2,B.GNUM_COUNTER_ID)
AS counter_Name,
''
AS clerk_Name,
TO_CHAR(GDT_ENTRY_DATE,'DD-MON-YYYY') AS date_data,
(HBLNUM_BILL_NO || '/' || HBLNUM_RECIEPT_NO) AS bill_No,
(NVL(B.HRGNUM_PUK,0)::CHARACTER VARYING)
AS cr_No,
INITCAP(DECODE(NVL(HRGNUM_PUK,0),0,'',AHIS_FUNCTION.FUN_PAT_NAME(HRGNUM_PUK)))
AS pat_Name,
(BILL_MST.GET_RECEIPT_NAME(HBLNUM_RECIEPT_TYPE) || '/'||
BILL_MST.GETBSERVICENAME(SBLNUM_BSERVICE_ID,0,B.GNUM_HOSPITAL_CODE))
AS  payment_Against,
 NVL(HRGNUM_AMT_COLLECTED,0) AS rece_Amt,DECODE(NVL(b.hrgnum_admission_no,0) , 0 , '' ,   b.hrgnum_admission_no ) ::character varying AS  adm_no
  FROM HBLT_DIRECT_CHARGE_DTL B]]></xml-property>
