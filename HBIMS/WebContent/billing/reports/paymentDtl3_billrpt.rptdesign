                        HBLSTR_PAYMENT_DTLS AS payment_dtls,(SELECT gstr_usr_name FROM GBLT_USER_MST WHERE gnum_userid = A.GNUM_SEATID AND gnum_isvalid = 1) AS clerk_Name
                        
                        FROM HBLT_BILL_RECIEPT_DTL A, HBLT_PAYMENT_DETAIL B
