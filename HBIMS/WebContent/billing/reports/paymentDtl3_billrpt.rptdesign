            <method name="beforeOpen"><![CDATA[	this.queryText="SELECT DECODE(NVL(A.HRGNUM_PUK,0),0,DECODE(NVL(A.HBLSTR_PREV_PUK,'0'),'0','-',A.HBLSTR_PREV_PUK),A.HRGNUM_PUK) AS cr_No, INITCAP(DECODE(NVL(A.HRGNUM_PUK,0),0,A.HBLSTR_PATIENT_NAME,Ahis_Function.fun_pat_name(A.HRGNUM_PUK))) AS pat_Name,TO_CHAR(A.HBLDT_RECIEPT_DATE,'DD-Mon-YYYY') AS date_val,(A.HBLNUM_BILL_NO || '/' || A.HBLNUM_RECIEPT_NO) AS bill_No, NVL(B.HBLNUM_RECIEPT_AMT,0) AS bill_Amt, Bill_Mst.getPaymentModeName(HBLNUM_PAYMENT_MODE,A.GNUM_HOSPITAL_CODE) AS payment_Mode, (Bill_Mst.get_receipt_name(A.HBLNUM_RECIEPT_TYPE) || '/' ||Bill_Mst.getbservicename(A.SBLNUM_BSERVICE_ID,0,A.GNUM_HOSPITAL_CODE)) AS  payment_Against, ";  
	this.queryText=this.queryText +"	SUBSTR(HBLSTR_PAYMENT_DTLS,1,25)   AS payment_dtls, ";
	this.queryText=this.queryText +"	(SELECT gstr_usr_name FROM GBLT_USER_MST WHERE gnum_userid = A.GNUM_SEATID AND gnum_isvalid = 1) AS clerk_Name,A.HBLDT_RECIEPT_DATE AS ORDER_DATE  FROM HBLT_BILL_RECIEPT_DTL A, HBLT_PAYMENT_DETAIL B WHERE A.GNUM_ISVALID = 1 AND B.GNUM_ISVALID = 1 AND A.HBLNUM_STATUS = 1 AND B.HBLNUM_PAYMENT_STATUS IN (0,1) AND A.GNUM_HOSPITAL_CODE = B.GNUM_HOSPITAL_CODE AND A.HBLNUM_RECIEPT_NO = B.HBLNUM_RECIEPT_NO AND A.HBLNUM_BILL_NO = B.HBLNUM_BILL_NO "; 
	this.queryText = this.queryText + " AND HBLNUM_PAYMENT_MODE<>DECODE(B.SBLNUM_BSERVICE_ID,19,10,20,10,0) AND A.GNUM_HOSPITAL_CODE IN (" +params["hosp_Code"] + ")" ;
	
	if(parseInt(params["charge_Type_Id"].value)>0)
	{
	this.queryText=this.queryText+"  AND A.SBLNUM_CHARGETYPE_ID = "+"'"+params["charge_Type_Id"]+"'";
	}
	
	
	if(parseInt(params["payment_mode_Id"].value)>0)
	{
	this.queryText=this.queryText+"  AND 	B.HBLNUM_PAYMENT_MODE IN (" +params["payment_mode_Id"] + ")" ;
	}
	
	
	
	this.queryText=this.queryText+" AND trunc(A.HBLDT_RECIEPT_DATE) >= to_date(to_char("  +  "'" +params["from_Date"]+"'::timestamp,'dd-mm-yyyy'),'dd-mm-yyyy')";
	
	this.queryText=this.queryText+" AND trunc(A.HBLDT_RECIEPT_DATE) <=  to_date(to_char("  +  "'" +params["to_Date"]+ "'::timestamp,'dd-mm-yyyy'),'dd-mm-yyyy')";
	//this.queryText=this.queryText+ " ORDER BY A.HBLDT_RECIEPT_DATE,A.HRGNUM_PUK";
	this.queryText=this.queryText+" UNION ";
	this.queryText=this.queryText+" SELECT A.HRGNUM_PUK ::character varying AS cr_No, INITCAP(Ahis_Function.fun_pat_name(A.HRGNUM_PUK)) AS pat_Name,TO_CHAR(A.gdt_entry_date,'DD-Mon-YYYY') AS date_val,(A.HBLNUM_BILL_NO || '/' || A.HBLNUM_RECIEPT_NO) AS bill_No, NVL(a.hrgnum_amt_collected,0) AS bill_Amt, Bill_Mst.getPaymentModeName(HBLNUM_PAYMENT_MODE,A.GNUM_HOSPITAL_CODE) AS payment_Mode, (Bill_Mst.get_receipt_name(A.HBLNUM_RECIEPT_TYPE) || '/' ||Bill_Mst.getbservicename(A.SBLNUM_BSERVICE_ID,0,A.GNUM_HOSPITAL_CODE))||'('|| bill_mst.gettariffname(a.hblstr_tariff_id::numeric, 0::numeric, a.GNUM_HOSPITAL_CODE::numeric) ||')' AS  payment_Against, HBLSTR_PAYMENT_DTLS AS payment_dtls,(SELECT gstr_usr_name FROM GBLT_USER_MST WHERE gnum_userid = A.GNUM_SEAT_ID AND gnum_isvalid = 1) AS clerk_Name,A.GDT_ENTRY_DATE AS ORDER_DATE ";
	this.queryText=this.queryText+ " FROM hblt_direct_charge_dtl a WHERE A.GNUM_ISVALID = 1  AND A.GNUM_HOSPITAL_CODE IN (" +params["hosp_Code"] + ")";
	
	if(parseInt(params["payment_mode_Id"].value)>0)
	{
	this.queryText=this.queryText+"  AND 	A.HBLNUM_PAYMENT_MODE IN (" +params["payment_mode_Id"] + ")" ;
	}
	
	if(parseInt(params["charge_Type_Id"].value)>0)
	{
	this.queryText=this.queryText+"  AND A.SBLNUM_CHARGETYPE_ID = "+"'"+params["charge_Type_Id"]+"'";
	}
	
	this.queryText=this.queryText+ " AND trunc(A.gdt_entry_date) >= to_date(to_char("  +  "'" +params["from_Date"]+"'::timestamp,'dd-mm-yyyy'),'dd-mm-yyyy')";
	this.queryText=this.queryText+ " AND trunc(A.gdt_entry_date) <=  to_date(to_char("  +  "'" +params["to_Date"]+ "'::timestamp,'dd-mm-yyyy'),'dd-mm-yyyy')";
	this.queryText=this.queryText+ " UNION "; 
	this.queryText=this.queryText+ " SELECT A.HRGNUM_PUK ::character varying AS cr_No,INITCAP(Ahis_Function.fun_pat_name(A.HRGNUM_PUK)) AS pat_Name,TO_CHAR(A.gdt_entry_date,'DD-Mon-YYYY') AS date_val,(A.HBLNUM_BILL_NO || '/' || A.HBLNUM_RECIEPT_NO) AS bill_No, NVL(a.hrgnum_amt_collected,0) AS bill_Amt, Bill_Mst.getPaymentModeName(HBLNUM_PAYMENT_MODE,A.GNUM_HOSPITAL_CODE) AS payment_Mode, (Bill_Mst.get_receipt_name(A.HBLNUM_RECIEPT_TYPE) || '/' ||Bill_Mst.getbservicename(A.SBLNUM_BSERVICE_ID,0,A.GNUM_HOSPITAL_CODE,A.SBLNUM_CHARGETYPE_ID))||'('|| bill_mst.gettariffname(a.hblstr_tariff_id::numeric, 0::numeric, a.GNUM_HOSPITAL_CODE::numeric) ||')' AS  payment_Against,HBLSTR_PAYMENT_DTLS AS payment_dtls,	 (SELECT gstr_usr_name FROM GBLT_USER_MST WHERE gnum_userid = A.GNUM_SEAT_ID AND gnum_isvalid = 1) AS clerk_Name,A.GDT_ENTRY_DATE AS ORDER_DATE";
	
	this.queryText=this.queryText+ " FROM hblt_direct_charge_hist_dtl a WHERE A.GNUM_ISVALID = 1 "; 
	this.queryText=this.queryText+ " AND A.GNUM_HOSPITAL_CODE IN (" +params["hosp_Code"] + ")";
	
	if(parseInt(params["payment_mode_Id"].value)>0)
	{
	this.queryText=this.queryText+"  AND 	A.HBLNUM_PAYMENT_MODE IN (" +params["payment_mode_Id"] + ")" ;
	}
	
	if(parseInt(params["charge_Type_Id"].value)>0)
	{
	this.queryText=this.queryText+"  AND A.SBLNUM_CHARGETYPE_ID = "+"'"+params["charge_Type_Id"]+"'";
	}
	
	this.queryText=this.queryText+ "AND trunc(A.gdt_entry_date) >= to_date(to_char("  +  "'" +params["from_Date"]+"'::timestamp,'dd-mm-yyyy'),'dd-mm-yyyy')";
	this.queryText=this.queryText+ "AND trunc(A.gdt_entry_date) <=  to_date(to_char("  +  "'" +params["to_Date"]+ "'::timestamp,'dd-mm-yyyy'),'dd-mm-yyyy')";
	this.queryText=this.queryText+ " order by ORDER_DATE,bill_No";
]]></method>
