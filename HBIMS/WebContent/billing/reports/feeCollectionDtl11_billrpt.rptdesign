            <method name="beforeOpen"><![CDATA[this.queryText= "(select  pkg_rpt_func.get_dept_name(B.GNUM_HOSPITAL_CODE::character varying,B.GNUM_DEPT_CODE::character varying,sysdate) AS Dept_Name, DECODE(TO_CHAR(HBLDT_RECIEPT_DATE,'DD-Mon-YYYY'),'00:00',TO_CHAR(B.GDT_ENTRY_DATE,'DD-Mon-YYYY'),TO_CHAR(HBLDT_RECIEPT_DATE,'DD-Mon-YYYY')) AS date_data,TRUNC(HBLDT_RECIEPT_DATE) AS DATE1, A.HBLNUM_BILL_NO AS BILLNO,A.HBLNUM_RECIEPT_NO AS RECNO, DECODE(NVL(A.HRGNUM_PUK , 0) , 0 , '-' , A.HRGNUM_PUK) AS cr_No, NVL(A.GSTR_TARIFF_EXTRA_INFO,BILL_MST.gettariffname(A.HBLNUM_TARIFF_ID,0 , A.GNUM_HOSPITAL_CODE )) AS tariff_name, A.HBLNUM_BILL_QTY as bill_qty, A.HBLNUM_NET_AMOUNT AS rece_Amt,  A.HBLNUM_BILL_NO ||'/'|| a.HBLNUM_RECIEPT_NO AS bill_No, INITCAP(DECODE(NVL(B.HRGNUM_PUK,0),0,HBLSTR_PATIENT_NAME,Ahis_Function.fun_pat_name(B.HRGNUM_PUK))) AS pat_Name, (SELECT NVL(HBLSTR_TARIFF_CODE,'0') FROM HBLT_TARIFF_MST WHERE HBLNUM_TARIFF_ID=A.HBLNUM_TARIFF_ID AND GNUM_HOSPITAL_CODE=A.GNUM_HOSPITAL_CODE AND GNUM_ISVALID=1) AS TARIFF_CODE,(SELECT gstr_usr_name FROM GBLT_USER_MST WHERE gnum_userid = A.GNUM_SEATID AND gnum_isvalid = 1) AS clerk_Name,DECODE(NVL(b.hastr_adm_no,0) , 0 , '' ,    b.hastr_adm_no ) ::character varying AS  adm_no FROM HBLT_BILL_SERVICE_DTL A,HBLT_BILL_RECIEPT_DTL  B WHERE A.GNUM_ISVALID=1  AND A.GNUM_HOSPITAL_CODE=B.GNUM_HOSPITAL_CODE AND A.GNUM_ISVALID=B.GNUM_ISVALID AND A.HBLNUM_BILL_NO=B.HBLNUM_BILL_NO AND A.HBLNUM_RECIEPT_NO=B.HBLNUM_RECIEPT_NO AND B.HBLNUM_STATUS = 1 ";
               
this.queryText = this.queryText + " AND BILL_MST.GET_BILL_PAYMENTMODE(B.HRGNUM_PUK,B.HBLNUM_BILL_NO,B. HBLNUM_RECIEPT_NO,B.GNUM_HOSPITAL_CODE)<>DECODE(B.SBLNUM_BSERVICE_ID,19,10,20,10,0)  AND A.GNUM_HOSPITAL_CODE IN ( "+params["hosp_Code"] + ")" ;

this.queryText=this.queryText+" AND trunc(HBLDT_RECIEPT_DATE) >= to_date(to_char("  +  "'" +params["from_Date"]+"'::timestamp,'dd-mm-yyyy'),'dd-mm-yyyy')";

this.queryText=this.queryText+"AND trunc(HBLDT_RECIEPT_DATE) <=  to_date(to_char("  +  "'" +params["to_Date"]+ "'::timestamp,'dd-mm-yyyy'),'dd-mm-yyyy')" ;

if(parseInt(params["charge_Type_Id"].value) > 0){
               this.queryText = this.queryText + " AND B.SBLNUM_CHARGETYPE_ID = "+  "'"  + params["charge_Type_Id"] +"'" ;
          }
          
if(parseInt(params["dept_Code"].value) > 0){
               this.queryText = this.queryText + " AND B.GNUM_DEPT_CODE = "+  "'"  + params["dept_Code"] +"'" ;
          }

if(parseInt(params["tariff_Id"].value) != 0){
               this.queryText = this.queryText + " AND A.HBLNUM_TARIFF_ID IN ( "+params["tariff_Id"] + ")" ;
          }  
          
if(parseInt(params["clerk_Id"].value) > 0){
              this.queryText = this.queryText + "  AND A.GNUM_SEATID =  "+  "'" + params["clerk_Id"]+ "'" ;
}

this.queryText=this.queryText+"ORDER BY DATE1,BILLNO,RECNO)";

this.queryText=this.queryText+" UNION ";

this.queryText=this.queryText+"(select  pkg_rpt_func.get_dept_name(C.GNUM_HOSPITAL_CODE::character varying,C.GNUM_DEPT_CODE::character varying,sysdate) AS Dept_Name, DECODE(TO_CHAR(GDT_ENTRY_DATE,'DD-Mon-YYYY'),'00:00',TO_CHAR(C.GDT_ENTRY_DATE,'DD-Mon-YYYY'),TO_CHAR(GDT_ENTRY_DATE,'DD-Mon-YYYY')) AS date_data,TRUNC(GDT_ENTRY_DATE) AS DATE1, C.HBLNUM_BILL_NO AS BILLNO,C.HBLNUM_RECIEPT_NO AS RECNO, DECODE(NVL(C.HRGNUM_PUK , 0) , 0 , '-' , C.HRGNUM_PUK) AS cr_No, BILL_MST.gettariffname(C.HBLSTR_TARIFF_ID,0 , C.GNUM_HOSPITAL_CODE ) AS tariff_name, '1' as bill_qty, C.HRGNUM_AMT_COLLECTED AS rece_Amt,  C.HBLNUM_BILL_NO ||'/'|| C.HBLNUM_RECIEPT_NO AS bill_No, INITCAP(DECODE(NVL(C.HRGNUM_PUK,0),0,'',Ahis_Function.fun_pat_name(C.HRGNUM_PUK))) AS pat_Name, (SELECT NVL(HBLSTR_TARIFF_CODE,'0') FROM HBLT_TARIFF_MST WHERE HBLNUM_TARIFF_ID=C.HBLSTR_TARIFF_ID AND GNUM_HOSPITAL_CODE=C.GNUM_HOSPITAL_CODE AND GNUM_ISVALID=1) AS TARIFF_CODE,(SELECT gstr_usr_name FROM GBLT_USER_MST WHERE gnum_userid = C.GNUM_SEAT_ID AND gnum_isvalid = 1) AS clerk_Name,DECODE(NVL(c.hrgnum_admission_no,0) , 0 , '' ,   c.hrgnum_admission_no ) ::character varying AS  adm_no FROM HBLT_DIRECT_CHARGE_DTL C WHERE C.GNUM_ISVALID=1 ";

this.queryText = this.queryText + "AND C.GNUM_HOSPITAL_CODE IN ( "+params["hosp_Code"] + ")" ;

this.queryText=this.queryText+" AND trunc(GDT_ENTRY_DATE) >= to_date(to_char("  +  "'" +params["from_Date"]+"'::timestamp,'dd-mm-yyyy'),'dd-mm-yyyy')";

this.queryText=this.queryText+"AND trunc(GDT_ENTRY_DATE) <=  to_date(to_char("  +  "'" +params["to_Date"]+ "'::timestamp,'dd-mm-yyyy'),'dd-mm-yyyy')" ;

if(parseInt(params["charge_Type_Id"].value) > 0){
               this.queryText = this.queryText + " AND C.SBLNUM_CHARGETYPE_ID = "+  "'"  + params["charge_Type_Id"] +"'" ;
          }
          
if(parseInt(params["dept_Code"].value) > 0){
               this.queryText = this.queryText + " AND C.GNUM_DEPT_CODE = "+  "'"  + params["dept_Code"] +"'" ;
          }

if(parseInt(params["tariff_Id"].value) != 0){
               this.queryText = this.queryText + " AND C.HBLSTR_TARIFF_ID IN ( "+params["tariff_Id"] + ")" ;
          } 
if(parseInt(params["clerk_Id"].value) > 0){
              this.queryText = this.queryText + "  AND C.GNUM_SEAT_ID =  "+  "'" + params["clerk_Id"]+ "'" ;
}
this.queryText=this.queryText+"ORDER BY DATE1,BILLNO,RECNO)"; 

this.queryText=this.queryText+" UNION ";

