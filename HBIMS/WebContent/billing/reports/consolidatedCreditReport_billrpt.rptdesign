            <xml-property name="queryText"><![CDATA[SELECT  0 AS CLTNAME,
HRGNUM_PUK AS CRNO,AHIS_FUNCTION.FUN_PAT_NAME (HRGNUM_PUK) AS PATIENT_NAME,
DECODE(NVL(HBLSTR_CREDIT_LETTER_REF_NO,'NA'),'null','NA','NULL','NA',NVL(HBLSTR_CREDIT_LETTER_REF_NO,'NA'))
||'^'||NVL(TO_CHAR(HBLDT_CREDIT_LETTER_DATE),'NA') AS LETTERNO_DT,
HBLNUM_BILL_NO || '/' || HBLNUM_RECIEPT_NO AS BILL_NO,
TO_CHAR(GDT_ENTRY_DATE,'DD/MM/YYYY HH24:MI:SS') AS BILL_DATE,
NVL(GSTR_TARIFF_EXTRA_INFO,BILL_MST.GETTARIFFNAMEWITHCODE(HBLNUM_TARIFF_ID,0,A.GNUM_HOSPITAL_CODE)) AS TRFNAME,
NVL(HBLNUM_BILL_QTY,0)  AS QTY,
NVL(HBLNUM_AMT_PAID_BY_CLIENT,0) AS AMOUNT, 
(SELECT GSTR_REQ_NO FROM SBLT_OUTBOUND_DTL B WHERE A.HBLNUM_BILL_NO=B.HBLNUM_TRANS_NO AND A.HBLNUM_RECIEPT_NO=B.HBLNUM_RECIEPT_NO AND A.GNUM_HOSPITAL_CODE=B.GNUM_HOSPITAL_CODE AND B.GNUM_ISVALID=1  AND B.HBLNUM_STATUS = 1) AS REQNO,
--pkg_drpt_util_ahiscl.get_inv_sample_no(HBLNUM_BILL_NO,gstr_tariff_id) AS REQNO,
(SELECT NVL(HRGNUM_BALANCE_CREDIT_LIMIT,0) FROM HRGT_CREDIT_PAT_DTL C WHERE C.HRGNUM_PUK=A.HRGNUM_PUK and GBL_ISVALID=1) :: character varying AS LETTERSUBDATE,
(select HBLSTR_PAYMENT_DTLS from  HBLT_PAYMENT_DETAIL F where  HBLNUM_BILL_NO=A.HBLNUM_BILL_NO ) AS PAYMENTDTL 
FROM HBLT_BILL_SERVICE_DTL A
WHERE GNUM_ISVALID = 1 AND HBLNUM_STATUS = 1 
AND HBLSTR_CREDIT_LETTER_REF_NO IS NOT NULL 
AND HBLSTR_CREDIT_LETTER_REF_NO<>'NA'
AND NVL(HBLNUM_AMT_PAID_BY_CLIENT,0)>0
AND HBLNUM_TARIFF_ID >= 0  --AND HRGNUM_PUK IN (331011700345540,331011700350926,331011700167404)
--ORDER BY 1,2,5,6,7,8 LIMIT 500
UNION ALL
SELECT  0 AS CLTNAME, HRGNUM_PUK AS CRNO,AHIS_FUNCTION.FUN_PAT_NAME (HRGNUM_PUK) AS PATIENT_NAME, DECODE(NVL(HBLSTR_CREDIT_LETTER_REF_NO,'NA'),'null','NA','NULL','NA',NVL(HBLSTR_CREDIT_LETTER_REF_NO,'NA'))||'^'||NVL(TO_CHAR(HBLDT_CREDIT_LETTER_DATE),'NA') AS LETTERNO_DT, HBLNUM_BILL_NO || '/' || HBLNUM_RECIEPT_NO AS BILL_NO, TO_CHAR(GDT_ENTRY_DATE,'DD/MM/YYYY HH24:MI:SS') AS BILL_DATE, BILL_MST.GETTARIFFNAMEWITHCODE(HBLSTR_TARIFF_ID,0,B.GNUM_HOSPITAL_CODE) AS TRFNAME, 1 AS QTY, NVL(HBLNUM_ACTUAL_AMOUNT,0) AS AMOUNT,  '' AS REQNO, (SELECT to_char(GDT_ENTRY_DATE,'DD/MM/YYYY HH24:MI:SS') FROM HRGT_CREDIT_PAT_DTL C WHERE C.HRGNUM_PUK=B.HRGNUM_PUK ORDER BY GDT_ENTRY_DATE ASC LIMIT 1) AS LETTERSUBDATE, (select HBLSTR_PAYMENT_DTLS from  HBLT_PAYMENT_DETAIL F where  HBLNUM_BILL_NO=B.HBLNUM_BILL_NO ):: CHARACTER VARYING AS PAYMENTDTL  FROM HBLT_DIRECT_CHARGE_HIST_DTL B WHERE GNUM_ISVALID = 1 AND HBLSTR_CREDIT_LETTER_REF_NO IS NOT NULL AND HBLSTR_CREDIT_LETTER_REF_NO<>'NA' AND HBLNUM_CREDIT_BILL_FLAG=1 AND HBLSTR_TARIFF_ID >= 0
ORDER BY 1,2,5,6,7,8 LIMIT 500]]></xml-property>
        </oda-data-set>
