this.queryText=this.queryText + " SELECT  nvl((select sum(hblnum_net_amount) from hblt_pataccount_service_dtl where hrgnum_puk=a.hrgnum_puk and hblnum_tariff_id>0 and hblnum_finalbill_flag!=1),0) AS CLTNAME, HRGNUM_PUK AS CRNO,AHIS_FUNCTION.FUN_PAT_NAME (HRGNUM_PUK) AS PATIENT_NAME, nvl((SELECT STRING_AGG(DISTINCT(HBLSTR_CREDIT_LETTER_REF_NO), ',') FROM hrgt_credit_pat_dtl  WHERE  GNUM_HOSPITAL_CODE=A.GNUM_HOSPITAL_CODE AND HRGNUM_PUK=A.HRGNUM_PUK and gbl_isvalid=1),'NA')||'^'||nvl((SELECT STRING_AGG(DISTINCT(TO_CHAR(HBLDT_CREDIT_LETTER_DATE)), ',') FROM hrgt_credit_pat_dtl  WHERE  GNUM_HOSPITAL_CODE=A.GNUM_HOSPITAL_CODE AND HRGNUM_PUK=A.HRGNUM_PUK and gbl_isvalid=1),'NA') AS LETTERNO_DT, HBLNUM_REQ_NO || '/' || HBLNUM_RECIEPT_NO AS BILL_NO, TO_CHAR(GDT_ENTRY_DATE,'DD/MM/YYYY HH24:MI:SS') AS BILL_DATE, NVL(GSTR_TARIFF_EXTRA_INFO, BILL_MST.GETTARIFFNAMEWITHCODE(HBLNUM_TARIFF_ID,0,A.GNUM_HOSPITAL_CODE)) AS TRFNAME, NVL(HBLNUM_BILL_QTY,0)  AS QTY, NVL(HBLNUM_NET_AMOUNT,0) AS AMOUNT,  (SELECT GSTR_REQ_NO FROM SBLT_OUTBOUND_DTL B WHERE A.HBLNUM_REQ_NO=B.HBLNUM_TRANS_NO AND A.HBLNUM_RECIEPT_NO=B.HBLNUM_RECIEPT_NO AND A.GNUM_HOSPITAL_CODE=B.GNUM_HOSPITAL_CODE AND B.GNUM_ISVALID=1  AND B.HBLNUM_STATUS = 1) AS REQNO, (SELECT SUM(NVL(HRGNUM_CREDIT_LIMIT,0)) FROM HRGT_CREDIT_PAT_DTL C WHERE C.HRGNUM_PUK=A.HRGNUM_PUK AND C.GNUM_HOSPITAL_CODE=A.GNUM_HOSPITAL_CODE AND GBL_ISVALID=1 GROUP BY C.HRGNUM_PUK ) AS LETTERSUBDATE, ";
this.queryText=this.queryText +"(select HBLSTR_PAYMENT_DTLS from  HBLT_PAYMENT_DETAIL F where trim(HBLNUM_BILL_NO|| '/' || HBLNUM_RECIEPT_NO) = TRIM(A.HBLNUM_REQ_NO || '/' || A.HBLNUM_RECIEPT_NO)  ) AS PAYMENTDTL  ";

this.queryText=this.queryText +" FROM HBLT_PATACCOUNT_SERVICE_DTL A WHERE GNUM_ISVALID = 1 AND HBLNUM_STATUS = 1  and sblnum_bservice_id Not IN(19,20,21) AND HBLNUM_TARIFF_ID >= 0 AND (SELECT COUNT(*) from HRGT_CREDIT_PAT_DTL where hrgnum_puk=a.hrgnum_puk and gbl_isvalid=1)>0";
this.queryText =this.queryText +"AND A.GNUM_HOSPITAL_CODE = "+  "'" +params["hosp_Code"] + "'" ;
