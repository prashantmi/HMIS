}
this.queryText=this.queryText+" AND  trunc(A.GDT_ENTRY_DATE) >= to_date(to_char("  +  "'" +params["from_Date"]+"'::timestamp,'dd-mm-yyyy')::timestamp,'dd-mm-yyyy')::timestamp";

this.queryText=this.queryText+" AND trunc(A.GDT_ENTRY_DATE) <=  to_date(to_char("  +  "'" +params["to_Date"]+ "'::timestamp,'dd-mm-yyyy')::timestamp,'dd-mm-yyyy')::timestamp" ;


this.queryText=this.queryText+" ORDER BY 1,2,5,6,7,8";]]></method>
