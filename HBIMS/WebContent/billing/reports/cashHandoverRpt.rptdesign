            <method name="beforeOpen"><![CDATA[this.queryText="select ";
this.queryText=this.queryText+" hblnum_summ_handover_no as scroll_no,(select sblstr_costtype_name::character varying from sblt_costtype_mst where sblnum_costtype_id=A.sblnum_handover_type_id limit 1) as handover_type,hblnum_payment_amount as amount,to_char(gdt_entry_date,'DD-Mon-YYYY') as summ_date,hblnum_reference_no as ref_no,(select sblstr_handover_detil::character varying from sblt_costtype_mst where sblnum_costtype_id=A.sblnum_handover_type_id limit 1) as handover_name";
this.queryText=this.queryText+" from hblt_dayend_handover_dtl A where";

if(params["cost_type"].value != 0){
this.queryText=this.queryText+"  sblnum_handover_type_id="+params["cost_type"].value +" AND ";
}


this.queryText=this.queryText+"  GNUM_HOSPITAL_CODE IN ( " +params["hosp_Code"] + ")" ;

this.queryText=this.queryText+" AND trunc(GDT_ENTRY_DATE) >= to_date(to_char("  +  "'" +params["from_Date"]+"'::timestamp,'dd-mm-yyyy'),'dd-mm-yyyy')";

this.queryText=this.queryText+" AND trunc(GDT_ENTRY_DATE) <=  to_date(to_char("  +  "'" +params["to_Date"]+ "'::timestamp,'dd-mm-yyyy'),'dd-mm-yyyy')" ;
]]></method>
