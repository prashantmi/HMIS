            <method name="beforeOpen"><![CDATA[this.queryText=" SELECT Pkg_Rpt_Func.get_dept_name(a.GNUM_HOSPITAL_CODE::character varying,a.GNUM_DEPT_CODE::character varying,a.GDT_ENTRY_DATE) AS Dept_Name, ";
