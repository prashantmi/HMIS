this.queryText=this.queryText+"  WHERE gnum_isvalid=1 ";
this.queryText=this.queryText+"  AND GNUM_HOSPITAL_CODE = a.GNUM_HOSPITAL_CODE ";
this.queryText=this.queryText+"  AND HIPNUM_WARD_CODE = a.HIPNUM_WARD_CODE) ";
this.queryText=this.queryText+"  AS ward_group_name, ";
this.queryText=this.queryText+"  Pkg_Rpt_Func.get_room_name(a.GNUM_HOSPITAL_CODE::character varying,a.HIPNUM_ROOM_CODE::character varying) AS room_No, ";
this.queryText=this.queryText+"  Pkg_Rpt_Func.get_bed_name(a.GNUM_HOSPITAL_CODE::character varying,a.HIPNUM_BED_CODE::character varying) AS bed_No, ";
this.queryText=this.queryText+"  Ahis_Function.fun_pat_name(a.HRGNUM_PUK) AS pat_Name ";
this.queryText=this.queryText+"  FROM HEPRT_DEATH_NOTIF_DTL A ";
this.queryText=this.queryText+"  WHERE a.GNUM_ISVALID = 1 ";

if(!parseInt(params["hosp_Code"]).value == 0)
