            <xml-property name="queryText"><![CDATA[SELECT A.HIPNUM_ADMNO, A.HRGNUM_PUK, ahis_util.PUK_NAME(A.HRGNUM_PUK) as HRGNUM_NAME,
    ipd_mst.sex(A.HRGNUM_PUK::character varying,A.GNUM_HOSPITAL_CODE::character varying) AS sex,ipd_mst.PUK_AGE(A.HRGNUM_PUK) AS age,
    TO_CHAR(A.HIPDT_ADMDATETIME,'DD-MM-YY /HH24:MI') AS ADM_DATE,
    TO_CHAR(A.HIPDT_DISDATETIME,'DD-MM-YY /HH24:MI') AS DESC_DATE,
    Pkg_Rpt_Func.get_dept_name(B.GNUM_HOSPITAL_CODE::character varying,B.GNUM_DEPT_CODE::character varying,B.GDT_ENTRY_DATE) AS Dept_Name,
    Pkg_Rpt_Func.get_deptunit_name(B.GNUM_HOSPITAL_CODE::character varying,B.GNUM_DEPTUNIT_CODE::character varying,B.GDT_ENTRY_DATE) AS Deptunit_Name,
    Pkg_Rpt_Func.get_ward_name(B.GNUM_HOSPITAL_CODE::character varying,B.HIPNUM_WARDCODE::character varying,B.GDT_ENTRY_DATE)AS ward_name,
    Pkg_Rpt_Func.get_room_name(B.GNUM_HOSPITAL_CODE::character varying,B.HIPNUM_ROOMNO::character varying) AS room_name ,
    ahis_gbl_util.EMP_NAME(A.HIPNUM_CONSULTANT_ID,A.GNUM_HOSPITAL_CODE) as emp_name,
	DECODE(B.HIPDT_TRANSIN_DATETIME,NULL,'No','Yes') ||'-'|| TO_CHAR(B.HIPDT_TRANSIN_DATETIME,'DD-MM-YY HH24:MI')  AS ACCEPTED  ,
	TO_CHAR(B.HIPDT_TRANSIN_DATETIME,'DD-MM-YY HH24:MI') AS ACCEPTEDDATETIME,
	Pkg_Rpt_Func.get_user_name(A.GNUM_HOSPITAL_CODE::character varying,A.GNUM_SEATID::character varying) AS user_name
    FROM hipt_patadmission_dtl A,HIPT_PAT_MOV_DTL B
    WHERE A.GNUM_ISVALID = 1
    and a.HRGNUM_PUK=b.HRGNUM_PUK
    and b.HIPNUM_MOV_NO=1
    and b.    HIPNUM_TRANSIN_FLG = 0
    and a.HIPNUM_ADMNO=b.HIPNUM_ADMNO
    and a.GNUM_HOSPITAL_CODE=b.GNUM_HOSPITAL_CODE
    and a.GNUM_ISVALID=b.GNUM_ISVALID]]></xml-property>
