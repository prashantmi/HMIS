            <method name="beforeOpen"><![CDATA[this.queryText=" SELECT A.HIPNUM_ADMNO, A.HRGNUM_PUK, AHIS_UTIL.PUK_NAME(A.HRGNUM_PUK) AS HRGNUM_NAME, ";
this.queryText=this.queryText+"  IPD_MST.SEX(A.HRGNUM_PUK::character varying,A.GNUM_HOSPITAL_CODE::character varying) AS SEX,IPD_MST.PUK_AGE(A.HRGNUM_PUK) AS AGE, ";
this.queryText=this.queryText+"  TO_CHAR(A.HIPDT_ADMDATETIME,'DD-MM-YY HH24:MI') AS ADM_DATE, ";
this.queryText=this.queryText+"  TO_CHAR(A.HIPDT_DISDATETIME,'DD-MM-YY HH24:MI') AS DESC_DATE, ";
this.queryText=this.queryText+"  PKG_RPT_FUNC.GET_DEPT_NAME(A.GNUM_HOSPITAL_CODE::character varying,B.GNUM_DEPT_CODE::character varying,A.GDT_ENTRY_DATE) AS DEPT_NAME, ";
this.queryText=this.queryText+"  PKG_RPT_FUNC.GET_DEPTUNIT_NAME(A.GNUM_HOSPITAL_CODE::character varying,B.GNUM_DEPTUNIT_CODE::character varying,A.GDT_ENTRY_DATE) AS DEPTUNIT_NAME, ";
this.queryText=this.queryText+"  PKG_RPT_FUNC.GET_WARD_NAME(A.GNUM_HOSPITAL_CODE::character varying,B.HIPNUM_WARDCODE::character varying,A.GDT_ENTRY_DATE)AS WARD_NAME, ";
this.queryText=this.queryText+"  PKG_RPT_FUNC.GET_ROOM_NAME(A.GNUM_HOSPITAL_CODE::character varying,B.HIPNUM_ROOMNO::character varying) AS ROOM_NAME , ";
this.queryText=this.queryText+"  AHIS_GBL_UTIL.EMP_NAME(A.HIPNUM_CONSULTANT_ID,A.GNUM_HOSPITAL_CODE) AS EMP_NAME, ";
this.queryText=this.queryText+"  TO_CHAR(B.HIPDT_TRANSIN_DATETIME,'DD-MM-YY HH24:MI') AS ACCEPTED,  "; 
this.queryText=this.queryText+"  TO_CHAR(B.HIPDT_TRANSIN_DATETIME,'DD-MM-YY HH24:MI') AS ACCEPTEDDATETIME,  ";
this.queryText=this.queryText+"  Pkg_Rpt_Func.get_user_name(A.GNUM_HOSPITAL_CODE::character varying,A.GNUM_SEATID::character varying) AS user_name ";
this.queryText=this.queryText+"  FROM HIPT_PATADMISSION_DTL A,HIPT_PAT_MOV_DTL B ";
this.queryText=this.queryText+"  WHERE A.GNUM_ISVALID = 1 ";
this.queryText=this.queryText+"  AND A.HRGNUM_PUK=B.HRGNUM_PUK ";
this.queryText=this.queryText+"  AND B.HIPNUM_MOV_NO=1 ";
this.queryText=this.queryText+"  AND B.HIPNUM_TRANSIN_FLG = 0 AND HIPNUM_ISACCEPTED=1";
this.queryText=this.queryText+"  AND HIPDT_ADMSTATUS_CODE NOT IN (13,14)  ";
this.queryText=this.queryText+"  AND B.HIPDT_TRANSIN_DATETIME IS NOT NULL ";
this.queryText=this.queryText+"  AND A.HIPDT_BEDALLOC_DATETIME IS NOT NULL ";
this.queryText=this.queryText+"  AND A.HIPNUM_ADMNO=B.HIPNUM_ADMNO ";
this.queryText=this.queryText+"  AND A.GNUM_HOSPITAL_CODE=B.GNUM_HOSPITAL_CODE ";
this.queryText=this.queryText+"  AND A.GNUM_ISVALID=B.GNUM_ISVALID ";
this.queryText=this.queryText+"  AND TRUNC(A.HIPDT_ADMDATETIME)>=TRUNC(AHIS_FUNCTION.FUN_STR_DATE('" +params["from_Date"]+"')) ";
this.queryText=this.queryText+"  AND TRUNC(A.HIPDT_ADMDATETIME)<TRUNC(AHIS_FUNCTION.FUN_STR_DATE('" +params["to_Date"]+"')+1) ";

if(!parseInt(params["hosp_Code"]) == 0)
{
this.queryText=this.queryText+" AND B.GNUM_HOSPITAL_CODE = '" +params["hosp_Code"] + "' ";
}
if(!parseInt(params["dept_Code"]) == 0)
{
this.queryText=this.queryText+" AND B.GNUM_DEPT_CODE  = '" +params["dept_Code"] + "' ";
}
if(!parseInt(params["unit_Code"]) == 0)
{
this.queryText=this.queryText+" AND B.GNUM_DEPTUNIT_CODE = '" +params["unit_Code"] + "' ";
}
if(!parseInt(params["ward_Code"])==  0)
{
this.queryText=this.queryText+" AND B.HIPNUM_WARDCODE ='" +params["ward_Code"] + "' ";
}
this.queryText=this.queryText+" AND A.GNUM_HOSPITAL_CODE = '" +params["hosp_Code"] + "' ORDER BY Dept_Name,ward_name, A.HIPDT_ADMDATETIME ";]]></method>
