            <xml-property name="queryText"><![CDATA[SELECT  B.PUK AS PUK,B.ADM_NO AS ADM_NO,B.MOVNO,AHIS_FUNCTION.FUN_PAT_NAME( B.PUK) AS PAT_NAME,
 ( select  A.hrgstr_age||'/'||DECODE(A.gstr_gender_code,1,'M',2,'F','Unknown')   from 
  hrgt_patient_dtl  A  WHERE A.HRGNUM_PUK = B.PUK 
 AND GNUM_HOSPITAL_CODE = B.HOSP_CODE
 AND GNUM_ISVALID=1 and rownum=1 )  AS sex,

( select Pkg_Rpt_Func.get_patcat_name(A.gnum_HOSPital_CODE::character varying,A.HIPNUM_TREAT_CATEG_CODE::character varying,A.HIPDT_ADMDATETIME) from 
   hipt_patadmission_dtl A 
   WHERE A.HRGNUM_PUK = B.PUK 
 AND A.GNUM_HOSPITAL_CODE = B.HOSP_CODE
 AND gnum_isvalid=1 and rownum=1) AS  CAT,

( SELECT HRGSTR_address_line1||' '||HRGSTR_sub_locality1||','||HRGSTR_CITY||'-'||HRGNUM_PINCODE 
    FROM HRGT_PATIENT_ADD_DTL
    WHERE HRGNUM_PUK=B.PUK 
    AND  GNUM_HOSPITAL_CODE=B.HOSP_CODE and gnum_isvalid=1 and rownum=1) as Address ,

( SELECT HIPDT_ADMDATETIME
 FROM HIPT_PATADMISSION_DTL
 WHERE HIPNUM_ADMNO=B.ADM_NO  and gnum_isvalid=1 and HRGNUM_PUK = B.PUK 
 AND GNUM_HOSPITAL_CODE = B.HOSP_CODE and rownum=1 ) ADMISSION_DATE,
 
 PKG_RPT_FUNC.GET_DEPT_NAME(B.HOSP_CODE::character varying,B.DC::character varying,SYSDATE) AS DEP_NAME,
 PKG_RPT_FUNC.GET_DEPT_NAME(B.HOSP_CODE::character varying,B.DC::character varying,SYSDATE)||'/'|| 
                    PKG_RPT_FUNC.GET_WARD_NAME(B.HOSP_CODE::character varying,B.PWC::character varying,SYSDATE)AS TRANSFER_FRM,
                    PKG_RPT_FUNC.GET_WARD_NAME(B.HOSP_CODE::character varying,B.PWC::character varying,SYSDATE)AS TRANSFER_FRM_FOR_GROUP,
                    ( SELECT PKG_RPT_FUNC.GET_DEPT_NAME(C.GNUM_HOSPITAL_CODE::character varying,C.GNUM_DEPT_CODE::character varying,SYSDATE)||'/'||
                    PKG_RPT_FUNC.GET_WARD_NAME (C.GNUM_HOSPITAL_CODE::character varying,C.HIPNUM_WARDCODE::character varying,SYSDATE)
                    FROM HIPT_PAT_MOV_DTL C
                    WHERE HRGNUM_PUK=B.PUK AND
                    HIPNUM_ADMNO=B.ADM_NO AND
                    GNUM_HOSPITAL_CODE=B.HOSP_CODE AND
                    C.HIPNUM_MOV_NO=B.MOVNO+1) TRANSFER_TO,
                    
   (SELECT PKG_RPT_FUNC.get_user_name (C.GNUM_HOSPITAL_CODE::character varying,C.GNUM_SEATID::character varying)
                    FROM HIPT_PAT_MOV_DTL C
                    WHERE HRGNUM_PUK=B.PUK AND
                    HIPNUM_ADMNO=B.ADM_NO AND
                    GNUM_HOSPITAL_CODE=B.HOSP_CODE AND
                    C.HIPNUM_MOV_NO=B.MOVNO+1) user_name,
                    
                    TO_CHAR(B.TOD ,'DD-Mon-YYYY') AS TRANSFER_DATE,
                    TO_CHAR(B.TOD ,'HH24:MI') AS TRANSFER_TIME,
                    '12-Jul-2000' AS from_Date, '21-Jul-2014' AS TO_DATE,
                    ----b.puk,b.tid,
                    DECODE(b.tof,1,'Bed Change',2,'Dept/Ward Change',
                    3,'Discharge',4,'Leave',5,'Ward Change',6,'Dept Change',7,'Trans. To Service Area',8,
                    'Trans. To Outside Hospital',9,'Join',10,'Trans. From Outside Hosp',11,'Trans. From Service Area',
                    'Trans. From Dept.') AS TRANS_STATUS,B.TID TRANFER_IN
                    FROM (
                            SELECT HIPNUM_ADMNO ADM_NO,HRGNUM_PUK PUK,HIPNUM_MOV_NO MOVNO,GNUM_HOSPITAL_CODE HOSP_CODE,
                            HIPDT_TRANS_DATETIME TD,HIPNUM_TRANSIN_FLG TIF,HIPDT_TRANSIN_DATETIME TID,
                            HIPNUM_TRANSOUT_FLG TOF,HIPDT_TRANSOUT_DATETIME TOD,
                            HIPNUM_WARDCODE PWC,HIPNUM_ROOMNO PR,HIPNUM_BED_CODE PBC,GNUM_DEPT_CODE DC,         
                            GNUM_DEPTUNIT_CODE DUC,
                            --DECODE(HIPNUM_TRANSOUT_FLG,3,coalesce((SELECT 'Y' FROM HIPT_PATADMISSION_DTL
                            --WHERE HIPNUM_IS_DEAD=1 AND
                            --HIPNUM_ADMNO=A.HIPNUM_ADMNO AND
                            --HRGNUM_PUK=A.HRGNUM_PUK),'N')) DEAD,
                            ( SELECT HIPDT_ADMDATETIME
                            FROM HIPT_PATADMISSION_DTL
                            WHERE HIPNUM_ADMNO=A.HIPNUM_ADMNO) ADM_DT,
                            S.DT STATS_DT
                            FROM HIPT_PAT_MOV_DTL A,(SELECT TO_DATE('12-Jul-2000','dd-mon-yyyy')+x DT
                            FROM PIVOT WHERE x<=TO_DATE('21-Jul-2014','dd-mon-yyyy') - TO_DATE('12-Jul-2000','dd-mon-yyyy') )  S
                            WHERE ((TRUNC(HIPDT_TRANS_DATETIME)=TRUNC(S.DT)) OR
                            (TRUNC(HIPDT_TRANSIN_DATETIME)=TRUNC(S.DT)) OR
                            (TRUNC(HIPDT_TRANSOUT_DATETIME)=TRUNC(S.DT))) 
                         ) B
                    WHERE DECODE(B.TID,NULL,'N',DECODE(TRUNC(B.TOD),TRUNC(B.STATS_DT),DECODE(B.TID,NULL,'N',DECODE(B.TOF,3,'N',1,'N','Y')),'N'))='Y' 
                    AND b.hosp_code='101' AND B.TOF IN (2,5,6) ]]></xml-property>
        </oda-data-set>
    </data-sets>
