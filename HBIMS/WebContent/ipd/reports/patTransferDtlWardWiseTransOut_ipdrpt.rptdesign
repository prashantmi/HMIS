            <method name="beforeOpen"><![CDATA[this.queryText =" SELECT B.PUK||' '::VARCHAR2 AS PUK,B.ADM_NO||' '::VARCHAR2 AS ADM_NO,B.MOVNO,AHIS_FUNCTION.FUN_PAT_NAME( B.PUK) AS PAT_NAME,";

this.queryText=this.queryText+"(select  A.hrgstr_age||'/'||A.gstr_gender_code  from ";
this.queryText=this.queryText+"  hrgt_patient_dtl  A  WHERE A.HRGNUM_PUK = B.PUK ";
this.queryText=this.queryText+" AND GNUM_HOSPITAL_CODE = B.HOSP_CODE";
this.queryText=this.queryText+" AND GNUM_ISVALID=1 and rownum=1 )  AS sex,";

this.queryText=this.queryText+" (select Pkg_Rpt_Func.get_patcat_name(A.gnum_HOSPital_CODE::character varying,A.HIPNUM_TREAT_CATEG_CODE::character varying,A.HIPDT_ADMDATETIME) from ";
this.queryText=this.queryText+"   hipt_patadmission_dtl A ";
this.queryText=this.queryText+"   WHERE A.HRGNUM_PUK = B.PUK ";
this.queryText=this.queryText+" AND A.GNUM_HOSPITAL_CODE = B.HOSP_CODE";
this.queryText=this.queryText+" AND gnum_isvalid=1 and rownum=1) AS  CAT,";

this.queryText=this.queryText+"(SELECT HRGSTR_address_line1||' '||HRGSTR_sub_locality1||','||HRGSTR_CITY||'-'||HRGNUM_PINCODE  ";
this.queryText=this.queryText+"     FROM HRGT_PATIENT_ADD_DTL";
this.queryText=this.queryText+"    WHERE HRGNUM_PUK=B.PUK and rownum=1 ";
this.queryText=this.queryText+"    AND  GNUM_HOSPITAL_CODE=B.HOSP_CODE and gnum_isvalid=1) as Address ,";

this.queryText=this.queryText+" TO_CHAR(( SELECT HIPDT_ADMDATETIME ";
this.queryText=this.queryText+"  FROM HIPT_PATADMISSION_DTL";
this.queryText=this.queryText+"  WHERE HIPNUM_ADMNO=B.ADM_NO  and rownum=1 and gnum_isvalid=1 and HRGNUM_PUK = B.PUK ";
this.queryText=this.queryText+"  AND GNUM_HOSPITAL_CODE = B.HOSP_CODE),'DD-MM-YY HH24:MI') ADMISSION_DATE,";

this.queryText =this.queryText +" PKG_RPT_FUNC.GET_DEPT_NAME(B.HOSP_CODE::character varying,B.DC::character varying,SYSDATE) AS DEP_NAME,";
this.queryText =this.queryText +"   PKG_RPT_FUNC.GET_DEPT_NAME(B.HOSP_CODE::character varying,B.DC::character varying,SYSDATE)||'/'|| ";
this.queryText =this.queryText +"   PKG_RPT_FUNC.GET_WARD_NAME(B.HOSP_CODE::character varying,B.PWC::character varying,SYSDATE)AS TRANSFER_FRM,";
this.queryText =this.queryText +"   PKG_RPT_FUNC.GET_WARD_NAME(B.HOSP_CODE::character varying,B.PWC::character varying,SYSDATE)AS TRANSFER_FRM_FOR_GROUP,";
this.queryText =this.queryText +"   (SELECT PKG_RPT_FUNC.GET_DEPT_NAME(C.GNUM_HOSPITAL_CODE::character varying,C.GNUM_DEPT_CODE::character varying,SYSDATE)||'/'||";
this.queryText =this.queryText +"    PKG_RPT_FUNC.GET_WARD_NAME (C.GNUM_HOSPITAL_CODE::character varying,C.HIPNUM_WARDCODE::character varying,SYSDATE)";
this.queryText =this.queryText +"    FROM HIPT_PAT_MOV_DTL C";
this.queryText =this.queryText +"    WHERE HRGNUM_PUK=B.PUK AND";
this.queryText =this.queryText +"    HIPNUM_ADMNO=B.ADM_NO AND";
this.queryText =this.queryText +"    GNUM_HOSPITAL_CODE=B.HOSP_CODE AND";
this.queryText =this.queryText +"    C.HIPNUM_MOV_NO=B.MOVNO+1) TRANSFER_TO,";

this.queryText =this.queryText +"   (SELECT PKG_RPT_FUNC.get_user_name (C.GNUM_HOSPITAL_CODE::character varying,C.GNUM_SEATID::character varying)";
this.queryText =this.queryText +"    FROM HIPT_PAT_MOV_DTL C";
this.queryText =this.queryText +"    WHERE HRGNUM_PUK=B.PUK AND";
this.queryText =this.queryText +"    HIPNUM_ADMNO=B.ADM_NO AND";
this.queryText =this.queryText +"    GNUM_HOSPITAL_CODE=B.HOSP_CODE AND";
this.queryText =this.queryText +"    C.HIPNUM_MOV_NO=B.MOVNO+1) user_name,";



this.queryText =this.queryText +"    TO_CHAR(B.TOD ,'DD-MM-YYYY HH24:MI') AS TRANSFER_DATE,";
this.queryText =this.queryText +"    TO_CHAR(B.TOD ,'DD-MM-YY HH24:MI') AS TRANSFER_TIME,";
this.queryText =this.queryText +"    '" +params["from_Date"] +"' AS from_Date, '" +params["to_Date"] +"' AS TO_DATE,";
this.queryText =this.queryText +"     b.puk,b.tid,";
this.queryText =this.queryText +"      DECODE(b.tof,1,'Bed Change',2,'Dept/Ward Change',";
this.queryText =this.queryText +"      3,'Discharge',4,'Leave',5,'Ward Change',6,'Dept Change',7,'Trans. To Service Area',8,";
this.queryText =this.queryText +"      'Trans. To Outside Hospital',9,'Join',10,'Trans. From Outside Hosp',11,'Trans. From Service Area',";
this.queryText =this.queryText +"      'Trans. From Dept.') AS TRANS_STATUS,B.TID TRANFER_IN";
this.queryText =this.queryText +"      FROM (";
this.queryText =this.queryText +"      SELECT HIPNUM_ADMNO ADM_NO,HRGNUM_PUK PUK,HIPNUM_MOV_NO MOVNO,GNUM_HOSPITAL_CODE HOSP_CODE,";
this.queryText =this.queryText +"      HIPDT_TRANS_DATETIME TD,HIPNUM_TRANSIN_FLG TIF,HIPDT_TRANSIN_DATETIME TID,";
this.queryText =this.queryText +"      HIPNUM_TRANSOUT_FLG TOF,HIPDT_TRANSOUT_DATETIME TOD,";
this.queryText =this.queryText +"      HIPNUM_WARDCODE PWC,HIPNUM_ROOMNO PR,HIPNUM_BED_CODE PBC,GNUM_DEPT_CODE DC,         ";
this.queryText =this.queryText +"      GNUM_DEPTUNIT_CODE DUC,";
this.queryText =this.queryText +"     (SELECT HIPDT_ADMDATETIME";
this.queryText =this.queryText +"      FROM HIPT_PATADMISSION_DTL";
this.queryText =this.queryText +"      WHERE HIPNUM_ADMNO=A.HIPNUM_ADMNO) ADM_DT,";
this.queryText =this.queryText +"      S.DT STATS_DT";
this.queryText =this.queryText +"      FROM HIPT_PAT_MOV_DTL A,(SELECT TO_DATE('" +params["from_Date"] +"','dd-mon-yyyy')+x DT";
this.queryText =this.queryText +"      FROM PIVOT WHERE x<=TO_DATE('" +params["to_Date"] +"','dd-mon-yyyy') - TO_DATE('" +params["from_Date"] +"','dd-mon-yyyy') )  S";
this.queryText =this.queryText +"     WHERE ((TRUNC(HIPDT_TRANS_DATETIME)=TRUNC(S.DT)) OR ";
this.queryText =this.queryText +"     (TRUNC(HIPDT_TRANSIN_DATETIME)=TRUNC(S.DT)) OR";
this.queryText =this.queryText +"     (TRUNC(HIPDT_TRANSOUT_DATETIME)=TRUNC(S.DT))) ";
this.queryText =this.queryText +"      ) B";
this.queryText =this.queryText +"      WHERE DECODE(B.TID,NULL,'N',DECODE(TRUNC(B.TOD),TRUNC(B.STATS_DT),DECODE(B.TID,NULL,'N',DECODE(B.TOF,3,'N',1,'N','Y')),'N'))='Y' ";
this.queryText =this.queryText +"    AND b.hosp_code= '" +params["hosp_Code"] +"'  AND B.TOF IN (2,5,6) ";

if(parseInt(params["transfer_fromward"].value) > 0)
{
                     this.queryText= this.queryText + " AND B.PWC = '" +params["transfer_fromward"] +"' ";
                     
}
if(parseInt(params["transfer_toward"].value) > 0)
{
                    
this.queryText =this.queryText +"   AND ( SELECT C.HIPNUM_WARDCODE";
this.queryText =this.queryText +"   FROM HIPT_PAT_MOV_DTL C WHERE HRGNUM_PUK = B.PUK ";
this.queryText =this.queryText +"   AND HIPNUM_ADMNO = B.ADM_NO";
this.queryText =this.queryText +"   AND GNUM_HOSPITAL_CODE = B.HOSP_CODE";
this.queryText =this.queryText +"   AND C.HIPNUM_MOV_NO = B.MOVNO + 1 ) = '" +params["transfer_toward"] +"'  ";
                     
}]]></method>
