	<queryString><![CDATA[select countold,countnew,datedif,ceil((countold+countnew)/datedif) dailAvg, deptname from
(SELECT   NVL ((SELECT   COUNT (a.hrgnum_puk)
                   FROM HRGT_EPISODE_DTL a
                  WHERE a.hrgnum_is_old = 1
                    AND a.gnum_isvalid = 1
                    AND c.hgnum_deptunitcode = a.hgnum_deptunitcode
                    AND a.gnum_hospital_code = $P{hospitalCode}
                    AND trunc(a.hrgdt_episode_date) >=
                                        trunc(TO_DATE ($P{fromDate}, 'dd-Mon-yyyy'))
                    AND trunc(a.hrgdt_episode_date) <=
                             TRUNC (TO_DATE ($P{toDate}, 'dd-Mon-yyyy'))
               GROUP BY a.gnum_dept_code, b.gstr_dept_name),
              0
             ) countold,
         NVL ((SELECT   COUNT (d.hrgnum_puk)
                   FROM HRGT_EPISODE_DTL d
                  WHERE d.hrgnum_is_old = 0
                    AND d.gnum_isvalid = 1
                    AND c.hgnum_deptunitcode = d.hgnum_deptunitcode
                    AND d.gnum_hospital_code = $P{hospitalCode}
                    AND trunc(d.hrgdt_episode_date) >=
                                        trunc(TO_DATE ($P{fromDate}, 'dd-Mon-yyyy'))
                    AND trunc(d.hrgdt_episode_date) <=
                             TRUNC (TO_DATE ($P{toDate}, 'dd-Mon-yyyy'))
               GROUP BY d.gnum_dept_code, b.gstr_dept_name),
              0
             ) countnew,
             DECODE((trunc(TO_DATE ($P{toDate}, 'dd-Mon-yyyy'))-trunc(TO_DATE ($P{fromDate}, 'dd-Mon-yyyy'))),0,1,
		(trunc(TO_DATE ($P{toDate}, 'dd-Mon-yyyy'))-trunc(TO_DATE ($P{fromDate}, 'dd-Mon-yyyy')))) datedif,
         initcap(b.gstr_dept_name) || '-' || initcap(c.hgstr_unitname) deptname
    FROM GBLT_DEPARTMENT_MST b, HGBT_UNIT_MST c
   WHERE c.hgnum_deptunitcode LIKE '%'
     AND b.gnum_dept_code = c.hgnum_dept_code
     AND c.hgnum_isgeneral LIKE '%'
     AND b.gdt_effective_frm <= TRUNC (SYSDATE)
     AND NVL (b.gdt_effective_to, SYSDATE + 1) >= TRUNC (SYSDATE)
     AND c.gdt_effective_frm <= TRUNC (SYSDATE)
     AND NVL (c.gdt_effective_to, SYSDATE + 1) >= TRUNC (SYSDATE)
     AND b.gnum_hospital_code =  $P{hospitalCode}
     AND b.gnum_isvalid = 1 AND c.GNUM_HOSPITAL_CODE=b.GNUM_HOSPITAL_CODE
     AND c.GNUM_ISVALID=1) data
ORDER BY deptname]]></queryString>

