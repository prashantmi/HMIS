	<queryString><![CDATA[/* Formatted on 2010/01/07 11:15 (Formatter Plus v4.8.7) */
SELECT (SELECT COUNT (a.hrgnum_puk)
          FROM HRGT_EPISODE_DIAGNOSIS_DTL a, HRGT_PATIENT_DTL d
         WHERE d.gnum_gender_code = 2
           AND a.gnum_hospital_code = b.gnum_hospital_code
           AND d.gnum_hospital_code = b.gnum_hospital_code
           AND a.gnum_isvalid = 1
           AND d.gnum_isvalid = 1
           AND a.hrgnum_puk = d.hrgnum_puk
           AND a.hbnum_isrepeat = 0
           AND a.hrgnum_diagnostic_code = b.hgbstr_disease_code
           AND TRUNC (a.hrgt_episode_date) >=
                                 TRUNC (TO_DATE ($P{fromDate}, 'dd-Mon-yyyy'))
           AND TRUNC (a.hrgt_episode_date) <=
                                TRUNC (TO_DATE ($P{toDate}, 'dd-Mon-yyyy'))
           AND a.hrgnum_episode_code IN (
                  SELECT DISTINCT P.hrgnum_episode_code
                             FROM HIPT_PATADMDISC_DTL P
                            WHERE P.gnum_hospital_code = b.gnum_hospital_code
                              AND P.gnum_isvalid = 1
                              AND P.hipdt_admstatus_code = 12)) admitfemale,
       (SELECT COUNT (a.hrgnum_puk)
          FROM HRGT_EPISODE_DIAGNOSIS_DTL a, HRGT_PATIENT_DTL d
         WHERE d.gnum_gender_code = 2
           AND a.gnum_hospital_code = b.gnum_hospital_code
           AND b.gnum_hospital_code = d.gnum_hospital_code
           AND a.gnum_isvalid = 1
           AND d.gnum_isvalid = 1
           AND a.hrgnum_puk = d.hrgnum_puk
           AND a.hbnum_isrepeat = 0
           AND a.hrgnum_diagnostic_code = b.hgbstr_disease_code
           AND TRUNC (a.hrgt_episode_date) >=
                                 TRUNC (TO_DATE ($P{fromDate}, 'dd-Mon-yyyy'))
           AND TRUNC (a.hrgt_episode_date) <=
                                TRUNC (TO_DATE ($P{toDate}, 'dd-Mon-yyyy'))
           AND a.hrgnum_episode_code IN (
                  SELECT DISTINCT P.hrgnum_episode_code
                             FROM HIPT_PATADMDISC_DTL P
                            WHERE P.gnum_hospital_code = b.gnum_hospital_code
                              AND P.gnum_isvalid = 1
                              AND P.hipnum_is_dead = 1)) deadfemale,
       (SELECT COUNT (a.hrgnum_puk)
          FROM HRGT_EPISODE_DIAGNOSIS_DTL a, HRGT_PATIENT_DTL d
         WHERE d.gnum_gender_code = 1
           AND a.gnum_hospital_code = b.gnum_hospital_code
           AND d.gnum_hospital_code = b.gnum_hospital_code
           AND a.gnum_isvalid = 1
           AND d.gnum_isvalid = 1
           AND a.hrgnum_puk = d.hrgnum_puk
           AND a.hbnum_isrepeat = 0
           AND a.hrgnum_diagnostic_code = b.hgbstr_disease_code
           AND TRUNC (a.hrgt_episode_date) >=
                                 TRUNC (TO_DATE ($P{fromDate}, 'dd-Mon-yyyy'))
           AND TRUNC (a.hrgt_episode_date) <=
                                TRUNC (TO_DATE ($P{toDate}, 'dd-Mon-yyyy'))
           AND a.hrgnum_episode_code IN (
                  SELECT DISTINCT P.hrgnum_episode_code
                             FROM HIPT_PATADMDISC_DTL P
                            WHERE P.gnum_hospital_code = b.gnum_hospital_code
                              AND P.gnum_isvalid = 1
                              AND P.hipdt_admstatus_code = 12)) admitmale,
       (SELECT COUNT (a.hrgnum_puk)
          FROM HRGT_EPISODE_DIAGNOSIS_DTL a, HRGT_PATIENT_DTL d
         WHERE d.gnum_gender_code = 1
           AND a.gnum_hospital_code = b.gnum_hospital_code
           AND b.gnum_hospital_code = d.gnum_hospital_code
           AND a.gnum_isvalid = 1
           AND d.gnum_isvalid = 1
           AND a.hrgnum_puk = d.hrgnum_puk
           AND a.hbnum_isrepeat = 0
           AND a.hrgnum_diagnostic_code = b.hgbstr_disease_code
           AND TRUNC (a.hrgt_episode_date) >=
                                 TRUNC (TO_DATE ($P{fromDate}, 'dd-Mon-yyyy'))
           AND TRUNC (a.hrgt_episode_date) <=
                                TRUNC (TO_DATE ($P{toDate}, 'dd-Mon-yyyy'))
           AND a.hrgnum_episode_code IN (
                  SELECT DISTINCT P.hrgnum_episode_code
                             FROM HIPT_PATADMDISC_DTL P
                            WHERE P.gnum_hospital_code = b.gnum_hospital_code
                              AND P.gnum_isvalid = 1
                              AND P.hipnum_is_dead = 1)) deadmmale,
       NVL (INITCAP (b.hgbstr_disease), '-') distype
  FROM HGBT_ICD_DISEASE_MST b
 WHERE b.hgbnum_disease_type_code like $P{disType}
   AND b.gnum_hospital_code = $P{hospitalCode}
   AND b.gnum_isvalid = 1
order by b.hgbstr_disease]]></queryString>

