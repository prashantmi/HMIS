	<queryString><![CDATA[select deptcode, department,  NVL(sum(admIn),0) AS tot_adm, NVL(sum(disIn),0) AS tot_disc, NVL(sum(patstay),0) AS tot_stay,  NVL(sum(isDead),0) AS tot_death, NVL(sum(in48Hr),0) AS tot_death_bel48hr
from
(
SELECT X.GNUM_DEPT_CODE AS deptcode, IPD_MST.GETDEPTNAME (X.GNUM_DEPT_CODE, X.GNUM_HOSPITAL_CODE) department,
        decode(sign(TRUNC(X.HIPDT_ADMDATETIME)-TO_DATE($P{fromDate},'dd-Mon-yyyy')),-1,0,1) admIn,
        decode(sign(TO_DATE($P{toDate},'dd-Mon-yyyy')-TRUNC(X.HIPDT_DISDATETIME)),-1,0,1) disIn,
    ( decode(sign(TO_DATE($P{toDate},'dd-Mon-yyyy')-TRUNC(X.HIPDT_DISDATETIME)),-1,TO_DATE($P{toDate},'dd-Mon-yyyy'),TRUNC(X.HIPDT_DISDATETIME))
    - decode(sign(TRUNC(X.HIPDT_ADMDATETIME)-TO_DATE($P{fromDate},'dd-Mon-yyyy')),-1,TO_DATE($P{fromDate},'dd-Mon-yyyy'),TRUNC(X.HIPDT_ADMDATETIME))) AS patStay,
       x.HIPNUM_IS_DEAD AS isDead
        , decode(x.HIPNUM_IS_DEAD,0,0,
            NVL((Select DECODE(sign(2-(TRUNC(HPMRDT_DEATH_DATETIME)-TRUNC(X.HIPDT_ADMDATETIME))),-1,0,1) from HPMRT_PAT_DEATH_DTL where HRGNUM_PUK=x.HRGNUM_PUK and GNUM_HOSPITAL_CODE=x.GNUM_HOSPITAL_CODE and GNUM_ISVALID=1),0))  in48Hr
    FROM HIPT_PATADMISSION_DTL X
   WHERE X.GNUM_ISVALID = 1
     AND X.GNUM_HOSPITAL_CODE = $P{hospitalCode}
     AND X.HIPNUM_ISACCEPTED =1
     AND X.GNUM_DEPT_CODE LIKE $P{departmentCode}
     AND (TRUNC(X.HIPDT_ADMDATETIME) BETWEEN TO_DATE($P{fromDate},'dd-Mon-yyyy') AND TO_DATE($P{toDate},'dd-Mon-yyyy')
     OR TRUNC(X.HIPDT_DISDATETIME) BETWEEN TO_DATE($P{fromDate},'dd-Mon-yyyy') AND TO_DATE($P{toDate},'dd-Mon-yyyy')))
     
     
     
GROUP BY deptcode, department
order by department]]></queryString>

