	<queryString><![CDATA[select   hrgnum_puk as crNo, hrgnum_admission_no as admNo,INITCAP(hrgstr_fname||' '||HRGSTR_MNAME||' '||HRGSTR_LNAME) as patName,
        gstr_gender_code||'/ '||hrgstr_age as gender_age, hrgstr_city||', '||hrgstr_state_name as patAdd,     
   nvl(to_char(hpmrdt_death_datetime,'dd-Mon-yyyy HH12:MI AM'),' ') AS death_date,
     hpmrstr_immediate_cause1 AS DEATH_CAUSE,
         gnum_hospital_code AS hospitalCode,
     pkg_clinical_gbl_fun.gethospitalname(a.gnum_hospital_code::character varying) AS hospitalname,
	pkg_clinical_gbl_fun.gethospitaladdress(a.gnum_hospital_code::character varying) AS hospitaladdr          
  from  hpmrt_pat_death_dtl a
 where  $P{allHospitalCode} like '%'||GNUM_HOSPITAL_CODE||'%'
   and  trunc(hpmrdt_death_datetime) >= trunc(to_date($P{fromDate},'dd-Mon-yyyy'))
   and  trunc(hpmrdt_death_datetime) <= trunc(to_date($P{toDate},'dd-Mon-yyyy'))
 order by hpmrdt_death_datetime desc,patName,hospitalname]]></queryString>

