	<parameter name="today_date" isForPrompting="false" class="java.util.Date"/>
	<queryString><![CDATA[/* Formatted on 2009/08/21 11:37 (Formatter Plus v4.8.7) */
SELECT DISTINCT a.hrgnum_puk, hrgstr_mlc_no,TO_CHAR(b.HRGDT_REGISTER_DATE,'DD-Mon-YYYY')regDate,
(SELECT s.hrgstr_status
                   FROM HRGT_PAT_STATUS_MST s
                  WHERE s.hrgnum_status_code = b.HGNUM_PAT_STATUS_CODE
                    AND s.gnum_hospital_code = gnum_hospital_code
                    AND s.gnum_isvalid = 1) opinion,
                INITCAP ((SELECT    hrgstr_fname
                                 || ' '
                                 || hrgstr_mname
                                 || ' '
                                 || hrgstr_lname
                            FROM HRGT_PATIENT_DTL
                           WHERE a.hrgnum_puk = hrgnum_puk
                             AND gnum_isvalid = 1
                             AND gnum_hospital_code = a.gnum_hospital_code)
                        ) NAME,
                TO_CHAR (hrgdt_mlc_date, 'dd-Mon-yyyy') mlc_date,
                NVL((SELECT    str_emp_full_name
                   FROM PIST_EMP_REGISTRATION_DTL
                  WHERE a.psstr_cmo_code = str_emp_no
                    AND gnum_isvalid = 1
                    AND gnum_hospital_code = a.gnum_hospital_code),'-')
                                                               psstr_cmo_code,
                   (SELECT gstr_gender_name
                      FROM GBLT_GENDER_MST
                     WHERE gnum_gender_code = a.gnum_gender_code
                       AND gnum_isvalid = 1
                       )
                || '/'
                || Ahis_Util.dob_age_on (a.hrgdt_dob, SYSDATE) sex_age,
                (SELECT gstr_patient_cat_name
                   FROM GBLT_PATIENT_CAT_MST
                  WHERE gnum_patient_cat_code =
                                             b.hgnum_patient_cat_code
                    AND gnum_isvalid = 1
                    AND gnum_hospital_code = a.gnum_hospital_code
                    AND gdt_effective_frm <= TRUNC (SYSDATE)
                    AND TRUNC (NVL (gdt_effective_to, SYSDATE)) >=
                                                               TRUNC (SYSDATE))
                                                                      pat_cat
           FROM HRGT_PATIENT_MLC_DTL a, HRGT_PATIENT_DTL b
          WHERE a.hrgnum_puk = b.hrgnum_puk
            AND a.gnum_isvalid = 1
            AND b.gnum_isvalid = 1
            AND hrgnum_s_no =
                   (SELECT MAX (hrgnum_s_no)
                      FROM HRGT_PATIENT_MLC_DTL
                     WHERE a.hrgnum_puk = hrgnum_puk
                       AND gnum_isvalid = a.gnum_isvalid
                       AND gnum_hospital_code = a.gnum_hospital_code
                       AND hrgnum_episode_code = a.hrgnum_episode_code
                       AND hrgnum_visit_no = a.hrgnum_visit_no)
            AND a.gnum_hospital_code = $P{hospitalCode}
            AND b.gnum_hospital_code = a.gnum_hospital_code
            AND trunc(hrgdt_mlc_date )>=
                                 TRUNC (TO_DATE ($P{fromDate}, 'dd-Mon-yyyy'))
            AND trunc(hrgdt_mlc_date) <=
                                 TRUNC (TO_DATE ($P{toDate}, 'dd-Mon-yyyy'))
       ORDER BY  regDate,name]]></queryString>

