	<queryString><![CDATA[/* Formatted on 2009/12/04 12:29 (Formatter Plus v4.8.7) */
SELECT   d.hgbstr_disease disease, COUNT (a.hrgnum_puk) COUNT,
         TO_CHAR (a.gdt_entry_date, 'Month-YYYY') MONTH
    FROM HGBT_ICD_DISEASE_MST d, HRGT_EPISODE_DIAGNOSIS_DTL a
   WHERE d.gnum_hospital_code = $P{hospitalCode}
     AND d.gnum_isvalid = 1
     AND a.gnum_isvalid = 1
     AND TRIM(UPPER(a.hrgnum_diagnostic_code)) LIKE TRIM(UPPER(d.hgbstr_disease_code))
     AND a.hbnum_isrepeat IN (0, 1)
     AND a.hrgnum_diagnosis_code_type = 0
     AND TRUNC (a.gdt_entry_date) >=
                                 TRUNC (TO_DATE ($P{fromDate}, 'dd-Mon-yyyy'))
     AND TRUNC (a.gdt_entry_date) <=
                                TRUNC (TO_DATE ($P{toDate}, 'dd-Mon-yyyy'))
GROUP BY d.hgbstr_disease, TO_CHAR (a.gdt_entry_date, 'Month-YYYY')  
ORDER BY MONTH]]></queryString>

