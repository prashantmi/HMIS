	<queryString><![CDATA[select  distinct 
        (SELECT gstr_dept_name
               FROM gblt_department_mst
              WHERE gnum_dept_code = hdm.GNUM_DEPT_CODE
                AND gnum_hospital_code = hdm.GNUM_HOSPITAL_CODE
                ) as dept,  
        nvl((ipd_mst.getWardName(hdm.HIPNUM_WARD_CODE,hdm.GNUM_HOSPITAL_CODE)),'-') as wardName,
        nvl(ipd_mst.GETROOMNAME(hdm.HIPNUM_ROOM_NO,hdm.GNUM_HOSPITAL_CODE),'-') as roomName,
        to_char(dt.d,'dd-Mon-yyyy') as date1,dt.d as d1,
        (select count(HRGNUM_PUK) 
           from HIPT_PATADMISSION_DTL
          where HIPDT_ADMSTATUS_CODE=12
            and GNUM_HOSPITAL_CODE=hdm.GNUM_HOSPITAL_CODE
            and GNUM_DEPT_CODE=hdm.GNUM_DEPT_CODE
            and HIPNUM_WARD_CODE=hdm.HIPNUM_WARD_CODE
            and HIPNUM_ROOM_CODE=hdm.HIPNUM_ROOM_NO
            and GNUM_ISVALID=1
            and trunc(HIPDT_ADMDATETIME)=dt.d) as admit_Pat,
        (select count(HRGNUM_PUK) 
           from HIPT_PATADMISSION_DTL
          where HIPDT_ADMSTATUS_CODE=16
            and GNUM_HOSPITAL_CODE=hdm.GNUM_HOSPITAL_CODE
            and GNUM_DEPT_CODE=hdm.GNUM_DEPT_CODE
            and HIPNUM_WARD_CODE=hdm.HIPNUM_WARD_CODE
            and HIPNUM_ROOM_CODE=hdm.HIPNUM_ROOM_NO
            and GNUM_ISVALID=1
            and trunc(HIPDT_DISDATETIME)=dt.d) as disCharge_Pat,
            
        (select count(HRGNUM_PUK) 
           from HIPT_PAT_MOV_DTL b
          where b.HIPNUM_TRANSIN_FLG IN (2,6)
            and b.GNUM_DEPT_CODE=hdm.GNUM_DEPT_CODE
            and b.HIPNUM_WARDCODE=hdm.HIPNUM_WARD_CODE
            and b.HIPNUM_ROOMNO=hdm.HIPNUM_ROOM_NO
            and b.GNUM_ISVALID=1
            and b.HIPNUM_MOV_NO= (select max(c.HIPNUM_MOV_NO)
                                    from HIPT_PAT_MOV_DTL c
                                   where c.HIPNUM_ADMNO=b.HIPNUM_ADMNO
                                     and c.GNUM_HOSPITAL_CODE=b.GNUM_HOSPITAL_CODE
                                  )
            and trunc(HIPDT_TRANSIN_DATETIME)=dt.d) as trans_In_Pat,
            
        (select count(HRGNUM_PUK) 
           from HIPT_PAT_MOV_DTL b
          where b.HIPNUM_TRANSOUT_FLG IN (2,6)
            and b.GNUM_DEPT_CODE=hdm.GNUM_DEPT_CODE
            and b.HIPNUM_WARDCODE=hdm.HIPNUM_WARD_CODE
            and b.HIPNUM_ROOMNO=hdm.HIPNUM_ROOM_NO
            and b.GNUM_ISVALID=1
            and b.HIPNUM_MOV_NO= (select max(c.HIPNUM_MOV_NO)
                                    from HIPT_PAT_MOV_DTL c
                                   where c.HIPNUM_ADMNO=b.HIPNUM_ADMNO
                                     and c.GNUM_HOSPITAL_CODE=b.GNUM_HOSPITAL_CODE
                                  )
            and trunc(HIPDT_TRANSOUT_DATETIME)=dt.d) as trans_Out_Pat,
            
        (select count(HRGNUM_PUK) 
           from HIPT_PATADMISSION_DTL
          where HIPDT_ADMSTATUS_CODE=13
            and GNUM_HOSPITAL_CODE=hdm.GNUM_HOSPITAL_CODE
            and GNUM_DEPT_CODE=hdm.GNUM_DEPT_CODE
            and HIPNUM_WARD_CODE=hdm.HIPNUM_WARD_CODE
            and HIPNUM_ROOM_CODE=hdm.HIPNUM_ROOM_NO
            and GNUM_ISVALID=1
            and trunc(GDT_ENTRY_DATE)=dt.d) as dead_Pat,
            
        (select count(a.HRGNUM_PUK) 
           from HIPT_PATADMISSION_DTL a, HIPT_PAT_MOV_DTL b
          where a.HIPDT_ADMSTATUS_CODE <> 16
            and b.HIPNUM_TRANSOUT_FLG NOT IN (2,6)
            and a.HIPNUM_ADMNO=b.HIPNUM_ADMNO
            and a.GNUM_HOSPITAL_CODE=b.GNUM_HOSPITAL_CODE
            and b.HIPNUM_MOV_NO= (select max(c.HIPNUM_MOV_NO)
                                    from HIPT_PAT_MOV_DTL c
                                   where c.HIPNUM_ADMNO=b.HIPNUM_ADMNO
                                     and c.GNUM_HOSPITAL_CODE=b.GNUM_HOSPITAL_CODE
                                  )
            and a.GNUM_HOSPITAL_CODE=hdm.GNUM_HOSPITAL_CODE
            and a.GNUM_DEPT_CODE=hdm.GNUM_DEPT_CODE
            and a.HIPNUM_WARD_CODE=hdm.HIPNUM_WARD_CODE
            and a.HIPNUM_ROOM_CODE=hdm.HIPNUM_ROOM_NO
            and a.GNUM_ISVALID=1) as patRemain,
        (SELECT trim(GNUM_HOSPITAL_CODE||' ') FROM gblt_hospital_mst WHERE gnum_hospital_code = hdm.gnum_hospital_code) AS hospitalCode,
        (SELECT gstr_hospital_name FROM gblt_hospital_mst WHERE gnum_hospital_code = hdm.gnum_hospital_code) AS hospitalname,
        (SELECT gstr_hospital_add1||' '|| gstr_hospital_add2||' '|| GSTR_CITY||' '|| gnum_pin_code 
           FROM gblt_hospital_mst
          WHERE gnum_hospital_code = hdm.gnum_hospital_code) AS hospitaladdr
  from  HIPT_DUWRBED_MST hdm,
        (SELECT TO_DATE($P{fromDate}, 'dd-Mon-yyyy') - 1 + rownum as d
        FROM all_objects
        WHERE TO_DATE($P{fromDate}, 'dd-Mon-yyyy') - 1 + rownum <= TO_DATE($P{toDate}, 'dd-Mon-yyyy')) dt
 where  hdm.GNUM_HOSPITAL_CODE like $P{hospitalCode}
   and  hdm.GNUM_DEPT_CODE like $P{departmentCode}
   and  hdm.HGNUM_DEPTUNITCODE like $P{unit}
   and  hdm.HIPNUM_WARD_CODE like $P{wardCode}
   and  hdm.HIPNUM_ROOM_NO like $P{roomCode}
  order by hospitalname,dept,wardName,roomName,d1]]></queryString>

