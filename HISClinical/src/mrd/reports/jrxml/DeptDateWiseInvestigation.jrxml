	<queryString><![CDATA[/* Formatted on 2009/11/16 11:18 (Formatter Plus v4.8.7) */
SELECT      (SELECT d.gstr_dept_name
               FROM GBLT_DEPARTMENT_MST d
              WHERE SUBSTR (b.hgnum_deptunitcode, 1, 3) =
                                               d.gnum_dept_code
                AND d.gnum_hospital_code = $P{hospitalCode}
                AND d.gnum_isvalid = 1
                )
         || '-'
         || (SELECT u.hgstr_unitname
               FROM HGBT_UNIT_MST u
              WHERE u.gnum_hospital_code = $P{hospitalCode}
                AND u.gnum_isvalid = 1
                AND u.hgnum_isgeneral LIKE $P{speciality}
                AND u.hgnum_deptunitcode = b.hgnum_deptunitcode
AND u.GNUM_DEPT_CODE IN (SELECT e.GNUM_DEPT_CODE FROM GBLT_DEPARTMENT_MST e 
				WHERE e.GNUM_HOSPITAL_CODE=$P{hospitalCode} AND e.GNUM_ISVALID=1
				 
               ))
                                                                AS department,
         c.gstr_lab_name, COUNT (a.hivtnum_req_dno) AS TOTAL_INV
    FROM HIVT_REQUISITION_DTL a,
         HIVT_REQUISITION_HEADER b,
         HIVT_LABORATORY_MST c
   WHERE SUBSTR (a.hivtnum_req_dno, 0,22) = b.hivnum_req_no
     AND c.gnum_lab_code = a.gnum_lab_code
     AND c.gnum_hospital_code = $P{hospitalCode}
     AND c.gnum_isvalid = 1
     AND TRUNC (a.hivt_entry_date) >=
                                 TRUNC (TO_DATE ($P{fromDate}, 'dd-Mon-yyyy'))
     AND TRUNC (a.hivt_entry_date) <=
                              TRUNC (TO_DATE ($P{toDate}, 'dd-Mon-yyyy'))
     AND b.hgnum_deptunitcode LIKE $P{unit} AND c.gnum_lab_code LIKE $P{labCode}
GROUP BY b.hgnum_deptunitcode, c.gnum_lab_code, c.gstr_lab_name
ORDER BY department, gstr_lab_name]]></queryString>

