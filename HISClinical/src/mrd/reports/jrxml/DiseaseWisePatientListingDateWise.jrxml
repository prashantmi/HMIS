	</parameter>
	<parameter name="allHospitalCode" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["101,102,104,108"]]></defaultValueExpression>
	</parameter>
	<parameter name="departmentCode" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["%"]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select  hedd.HRGNUM_PUK,nvl(to_char(hedd.HRGNUM_ADMISSION_NO),'-') as HRGNUM_ADMISSION_NO,
        INITCAP(hpd.HRGSTR_FNAME||' '||hpd.HRGSTR_MNAME||' '||hpd.HRGSTR_LNAME) as patName,
        hpd.GSTR_GENDER_CODE  sex,
        ahis_util.dob_age_on (hpd.HRGDT_DOB, SYSDATE) age,
        hedd.HRGNUM_DIAGNOSTIC_CODE diseaseCode, 
      pkg_clinical_gbl_fun.get_icd_disease_name('100',hedd.HRGNUM_DIAGNOSTIC_CODE) as diseaseName,
        (select ipd_mst.GETDEPTNAME(GNUM_DEPT_CODE,hedd.GNUM_HOSPITAL_CODE)
           from hrgt_episode_dtl hed
          where hedd.HRGNUM_PUK= hed.HRGNUM_PUK
            and hedd.HRGNUM_EPISODE_CODE=hed.HRGNUM_EPISODE_CODE
            and hed.GNUM_ISVALID=1
            and hedd.HRGNUM_VISIT_NO=hed.HRGNUM_VISIT_NO
            and hedd.GNUM_HOSPITAL_CODE=hed.GNUM_HOSPITAL_CODE
        )as dept,
	hedd.gnum_hospital_code AS hospitalCode,
        pkg_clinical_gbl_fun.gethospitalname(hedd.gnum_hospital_code::character varying) AS hospitalname,
	pkg_clinical_gbl_fun.gethospitaladdress(hedd.gnum_hospital_code::character varying) AS hospitaladdr 
  from  HRGT_EPISODE_DIAGNOSIS_DTL hedd, HRGT_PATIENT_DTL hpd
 where  hedd.HRGNUM_PUK= hpd.HRGNUM_PUK
   and  hedd.GNUM_ISVALID=1
   and  (select GNUM_DEPT_CODE
           from hrgt_episode_dtl hed
          where hedd.HRGNUM_PUK= hed.HRGNUM_PUK
            and hedd.HRGNUM_EPISODE_CODE=hed.HRGNUM_EPISODE_CODE
            and hed.GNUM_ISVALID=1
            and hedd.HRGNUM_VISIT_NO=hed.HRGNUM_VISIT_NO
            and hedd.GNUM_HOSPITAL_CODE=hed.GNUM_HOSPITAL_CODE
        )  like $P{departmentCode}
   and  $P{allHospitalCode} like '%'||hedd.gnum_hospital_code||'%'
   and  trunc(hedd.GDT_ENTRY_DATE) >= trunc(to_date($P{fromDate},'dd-Mon-yyyy'))
   and  trunc(hedd.GDT_ENTRY_DATE) <= trunc(to_date($P{toDate},'dd-Mon-yyyy'))
  
 order  by hedd.GNUM_HOSPITAL_CODE, dept, hedd.HRGNUM_PUK, hedd.HRGNUM_DIAGNOSTIC_CODE,diseaseName]]></queryString>

