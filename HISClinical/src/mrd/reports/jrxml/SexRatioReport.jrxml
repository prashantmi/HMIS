	<queryString><![CDATA[SELECT  DISTINCT (SELECT COUNT(*)
	 FROM HIPT_PATADMDISC_DTL
         WHERE GNUM_ISVALID = a.GNUM_ISVALID and
	 gnum_hospital_code =a.gnum_hospital_code and
	 hrgstr_sex=1 and HIPNUM_ISNEWBORN=1 and
         TRUNC (hipdt_admdatetime  ) >=
                                 TRUNC (TO_DATE ($P{fromDate}, 'dd-Mon-yyyy')) and
	 TRUNC (hipdt_admdatetime  ) <=
                                TRUNC (TO_DATE ($P{toDate}, 'dd-Mon-yyyy'))) MALE_CHILD,
       (SELECT COUNT(*)
	 FROM HIPT_PATADMDISC_DTL
         WHERE GNUM_ISVALID = a.GNUM_ISVALID and
	 gnum_hospital_code =a.gnum_hospital_code and
	 hrgstr_sex=2 and HIPNUM_ISNEWBORN=1 and
	 TRUNC (hipdt_admdatetime  ) >=
                                 TRUNC (TO_DATE ($P{fromDate}, 'dd-Mon-yyyy')) and
	 TRUNC (hipdt_admdatetime  ) <=
                                TRUNC (TO_DATE ($P{toDate}, 'dd-Mon-yyyy')) ) FEMALE_CHILD
 FROM HIPT_PATADMDISC_DTL a
 WHERE	
	a.GNUM_ISVALID = 1 and

	a.gnum_hospital_code = $P{hospitalCode} and 

	
	TRUNC (a.hipdt_admdatetime  ) >=
                                 TRUNC (TO_DATE ($P{fromDate}, 'dd-Mon-yyyy')) and
	TRUNC (a.hipdt_admdatetime  ) <=
                                TRUNC (TO_DATE ($P{toDate}, 'dd-Mon-yyyy'))
 
	-- order by a.hipdt_admdatetime desc]]></queryString>

