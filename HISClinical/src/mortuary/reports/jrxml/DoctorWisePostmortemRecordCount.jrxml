	<queryString><![CDATA[select drcode , drname , count(postno) AS cnt from
(SELECT (SELECT  nvl(b.str_emp_full_name,'')
          FROM PIST_EMP_registration_DTL b
         WHERE b.str_emp_no = a.hmrstr_emp_id
           AND b.gnum_hospital_code = a.gnum_hospital_code
           AND b.gnum_isvalid = a.gnum_isvalid
        ) drname,
       a.hmrstr_emp_id drcode, a.hmrnum_postmortem_id postno
  FROM HMRT_POSTMORTEM_TEAM_DTL a,
       HMRT_POSTMORTEM_REQ_DTL c,
       HMRT_DECEASED_DTL d,
       HMRT_POSTMORTEM_DTL F
 WHERE a.hmrnum_postmortem_id = c.hmrnum_postmortem_id
   AND c.hmrnum_deceased_no = d.hmrnum_deceased_no
   AND F.hmrnum_postmortem_id = a.hmrnum_postmortem_id
   AND a.gnum_hospital_code = $P{hospitalCode}
   AND a.gnum_isvalid = 1
   AND a.gnum_hospital_code = c.gnum_hospital_code
   AND a.gnum_hospital_code = d.gnum_hospital_code
   AND a.gnum_hospital_code = F.gnum_hospital_code
   AND a.gnum_isvalid = c.gnum_isvalid
   AND a.gnum_isvalid = d.gnum_isvalid
   AND a.gnum_isvalid = F.gnum_isvalid
   AND a.hmrstr_emp_id LIKE $P{empNo}
   AND trunc(a.gdt_entry_date) >= TO_DATE ($P{fromDate}, 'dd-Mon-yyyy')
   AND trunc(a.gdt_entry_date) <= TRUNC (TO_DATE ($P{toDate}, 'dd-Mon-yyyy')) 
   AND  a.hmrnum_incharge = 1
   AND a.HMRNUM_IS_PERFORMED=2
   AND F.HRGNUM_SRNO=(SELECT MAX(g.HRGNUM_SRNO) FROM HMRT_POSTMORTEM_DTL g WHERE g.GNUM_HOSPITAL_CODE=F.GNUM_HOSPITAL_CODE
   AND g.GNUM_ISVALID=F.GNUM_ISVALID AND g.HMRNUM_POSTMORTEM_ID=F.HMRNUM_POSTMORTEM_ID)
   AND c.HMRNUM_POSTMORTEM_STATUS=6
   order by d.hmrstr_fname)
   group by drcode, drname
   order by drcode, drname]]></queryString>

