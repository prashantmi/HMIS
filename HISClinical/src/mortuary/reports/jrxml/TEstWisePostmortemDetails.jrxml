	<queryString><![CDATA[/* Formatted on 2009/10/23 11:25 (Formatter Plus v4.8.7) */
SELECT   TO_CHAR (a.gdt_entry_date, 'DD-Mon-YYYY') reqdate,NVL(initcap(a.HMRSTR_RESULT),'-')result,
         (SELECT c.hmrstr_ext_labtest_name
            FROM HMRT_EXT_LABTEST_MST c
           WHERE c.hmrnum_ext_labtest_id =
                                        a.hmrnum_ext_labtest_id
             AND a.gnum_isvalid = c.gnum_isvalid
             AND a.gnum_hospital_code = c.gnum_hospital_code) AS extlabtest,
         b.hmrnum_postmortem_id pmno,
         (SELECT    INITCAP (F.hmrstr_fname)
                 || ' '
                 || INITCAP (F.hmrstr_mname)
                 || ' '
                 || INITCAP (F.hmrstr_lname)
            FROM HMRT_DECEASED_DTL F
           WHERE F.gnum_hospital_code = a.gnum_hospital_code
             AND F.gnum_isvalid = a.gnum_isvalid
             AND F.hmrnum_deceased_no =
                    (SELECT e.hmrnum_deceased_no
                       FROM HMRT_POSTMORTEM_REQ_DTL e
                      WHERE e.hmrnum_postmortem_id = b.hmrnum_postmortem_id
                        AND e.hrgnum_srno =
                               (SELECT MAX (g.hrgnum_srno)
                                  FROM HMRT_POSTMORTEM_REQ_DTL g
                                 WHERE g.hmrnum_postmortem_id =
                                                        e.hmrnum_postmortem_id)))
                                                                      decname
    FROM HMRT_EXTLAB_INVREQ_DTL a, HMRT_EXTLAB_REQ_DTL b
   WHERE a.hmrnum_req_id = b.hmrnum_req_id
     AND a.gnum_isvalid = b.gnum_isvalid
     AND a.gnum_hospital_code = b.gnum_hospital_code
     AND a.gnum_isvalid = 1
     AND a.gnum_hospital_code = $P{hospitalCode}
     AND b.gdt_entry_date >= TO_DATE ($P{fromDate}, 'dd-Mon-yyyy')
     AND b.gdt_entry_date <= TRUNC (TO_DATE ($P{toDate}, 'dd-Mon-yyyy') + 1)
     AND a.hmrnum_ext_labtest_id LIKE $P{labTestId}
ORDER BY decname]]></queryString>

