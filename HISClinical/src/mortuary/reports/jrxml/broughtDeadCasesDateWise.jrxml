	<queryString><![CDATA[/* Formatted on 2009/10/22 11:45 (Formatter Plus v4.8.7) */
SELECT NVL((SELECT TO_CHAR(b.hmrnum_postmortem_id)
          FROM HMRT_POSTMORTEM_REQ_DTL b
         WHERE a.hmrnum_deceased_no = b.hmrnum_deceased_no
           AND a.gnum_hospital_code = b.gnum_hospital_code
           AND a.gnum_isvalid = b.gnum_isvalid
           AND b.hrgnum_srno =
                  (SELECT MAX (F.hrgnum_srno)
                     FROM HMRT_POSTMORTEM_REQ_DTL F
                    WHERE F.gnum_hospital_code = b.gnum_hospital_code
                      AND F.gnum_isvalid = b.gnum_isvalid
                      AND F.hmrnum_postmortem_id = b.hmrnum_postmortem_id)), '-')
                                                                          pid,
          INITCAP (a.hmrstr_fname)
       || ''
       || INITCAP (a.hmrstr_mname)
       || ''
       || INITCAP (a.hmrstr_lname) AS decname,
TO_CHAR(DECODE(a.hrgnum_puk,null, DECODE(HMRSTR_CRNO,NULL,'-',HMRSTR_CRNO||'(E)'),a.hrgnum_puk)) AS deccrno,
     NVL ((SELECT INITCAP (d.hgstr_death_manner_desc)
               FROM HGBT_DEATH_MANNER_MST d,
                    HMRT_POSTMORTEM_DTL c,
                    HMRT_POSTMORTEM_REQ_DTL e
              WHERE c.hgnum_death_manner_code = d.hgnum_death_manner_code
                AND c.gnum_hospital_code = a.gnum_hospital_code
                AND c.gnum_isvalid = a.gnum_isvalid
                AND d.gnum_isvalid = a.gnum_isvalid
                AND e.hmrnum_deceased_no = a.hmrnum_deceased_no
                AND e.gnum_isvalid = a.gnum_isvalid AND c.HMRNUM_POSTMORTEM_ID=e.HMRNUM_POSTMORTEM_ID
                AND e.gnum_hospital_code = a.gnum_hospital_code AND e.HRGNUM_SRNO=(SELECT MAX (g.hrgnum_srno)
                     FROM HMRT_POSTMORTEM_REQ_DTL g
                    WHERE g.gnum_hospital_code = a.gnum_hospital_code
                      AND g.gnum_isvalid = a.gnum_isvalid
                      AND g.HMRNUM_DECEASED_NO = a.HMRNUM_DECEASED_NO)),
            '-'
           ) AS deathmanner
  FROM HMRT_DECEASED_DTL a
 WHERE a.gnum_hospital_code LIKE $P{hospitalCode}
   AND a.gnum_isvalid = 1
   AND a.hmrnum_isbrought_dead = 1 
--AND a.HRGNUM_PUK IS NOT NULL
    AND trunc(a.gdt_entry_date) >= TO_DATE ($P{fromDate}, 'dd-Mon-yyyy')
   AND trunc(a.gdt_entry_date) <= TRUNC (TO_DATE ($P{toDate}, 'dd-Mon-yyyy')) 
   ORDER BY a.hmrstr_fname]]></queryString>

