	<parameter name="count" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT COUNT(a.HGNUM_DEATH_MANNER_CODE)AS count,(SELECT INITCAP(b.HGSTR_DEATH_MANNER_NAME)
FROM HGBT_DEATH_MANNER_MST b WHERE a.HGNUM_DEATH_MANNER_CODE=b.HGNUM_DEATH_MANNER_CODE
AND b.GNUM_ISVALID=1) deathManner FROM  
HMRT_POSTMORTEM_DTL a
WHERE   a.GNUM_HOSPITAL_CODE=$P{hospitalCode} AND 
a.GNUM_ISVALID=1 AND a.HGNUM_DEATH_MANNER_CODE LIKE $P{deathManner} 
AND a.gdt_entry_date >= TO_DATE ($P{fromDate}, 'dd-Mon-yyyy')
AND a.gdt_entry_date <= TRUNC (TO_DATE ($P{toDate}, 'dd-Mon-yyyy') + 1) 
 GROUP BY a.HGNUM_DEATH_MANNER_CODE
ORDER BY deathManner]]></queryString>

