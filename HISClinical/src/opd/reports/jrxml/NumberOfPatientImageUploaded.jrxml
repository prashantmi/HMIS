	<queryString><![CDATA[select distinct a.HRGNUM_PUK, (select HRGSTR_FNAME||' '||HRGSTR_MNAME||' '||HRGSTR_LNAME from HRGT_PATIENT_DTL where HRGNUM_PUK=a.HRGNUM_PUK AND GNUM_HOSPITAL_CODE=A.GNUM_HOSPITAL_CODE)name,
HOPSTR_IMAGE_NAME as fileName, (select GSTR_USR_NAME from GBLT_USER_MST where  GNUM_USERID=a.GNUM_SEAT_ID and GNUM_ISVALID=1  AND GNUM_HOSPITAL_CODE=A.GNUM_HOSPITAL_CODE)uploaded_by,
to_char(a.GDT_ENTRY_DATE,'dd-mon-yyyy')upload_date, (select GSTR_DEPT_NAME from GBLT_DEPARTMENT_MST where GNUM_DEPT_CODE=b.GNUM_DEPT_CODE  AND GNUM_HOSPITAL_CODE=A.GNUM_HOSPITAL_CODE
AND GDT_EFFECTIVE_FRM<=TRUNC(SYSDATE) AND NVL(GDT_EFFECTIVE_TO,SYSDATE+1)>=TRUNC(SYSDATE))dept,
a.GDT_ENTRY_DATE from HOPT_PAT_IMAGE_DTL a, HRGT_EPISODE_DTL b where a.GNUM_ISVALID=1 and b.GNUM_ISVALID=1
and a.GNUM_HOSPITAL_CODE=$P{hospitalCode} and b.GNUM_HOSPITAL_CODE=$P{hospitalCode} and a.HRGNUM_EPISODE_CODE=b.HRGNUM_EPISODE_CODE
and a.HRGNUM_PUK=b.HRGNUM_PUK and a.GDT_ENTRY_DATE >= to_date($P{fromDate},'dd-mon-yyyy')
and a.GDT_ENTRY_DATE <= trunc(to_date($P{toDate},'dd-mon-yyyy')+1) order by a.GDT_ENTRY_DATE,dept]]></queryString>

