	<queryString><![CDATA[SELECT GSTR_DEPT_NAME,HGSTR_UNITNAME,GSTR_PATIENT_CAT_NAME,

NVL((SELECT NVL(SUM(DECODE(AHIS_UTIL.PUK_TYPE(HRGNUM_PUK,12),10,1,0)),0)+
NVL(SUM(DECODE(AHIS_UTIL.PUK_TYPE(HRGNUM_PUK,12),20,1,0)),0)+
NVL(SUM(DECODE(AHIS_UTIL.PUK_TYPE(HRGNUM_PUK,12),11,1,0)),0)+
NVL(SUM(DECODE(AHIS_UTIL.PUK_TYPE(HRGNUM_PUK,12),21,1,0)),0)
FROM HRGT_EPISODE_DTL
WHERE HRGNUM_IS_OLD=1
AND HRGNUM_IS_CONFIRMED=2
AND HGNUM_DEPTUNITCODE=A.HGNUM_DEPTUNITCODE
AND GNUM_DEPT_CODE=B.GNUM_DEPT_CODE
AND HGNUM_PATIENT_CAT_CODE=C.GNUM_PATIENT_CAT_CODE
AND GNUM_HOSPITAL_CODE=A.GNUM_HOSPITAL_CODE
AND HRGDT_NEXT_VISIT_DATE=TO_DATE($P{toDate},'dd-Mon-yyyy')
GROUP BY HRGDT_NEXT_VISIT_DATE),0)OLD_PAT

FROM HGBT_UNIT_MST A,GBLT_DEPARTMENT_MST B,GBLT_PATIENT_CAT_MST C

WHERE B.GNUM_DEPT_CODE LIKE ($P{departmentCode})
AND (A.HGNUM_DEPTUNITCODE LIKE ($P{unit}) AND A.HGNUM_DEPT_CODE LIKE ($P{departmentCode}))
AND C.GNUM_PATIENT_CAT_CODE LIKE ($P{patPrimaryCatCode})
AND C.GNUM_PATIENT_CAT_TYPE=0
AND  (A.GDT_EFFECTIVE_FRM<=TRUNC(SYSDATE) AND NVL(A.GDT_EFFECTIVE_TO,SYSDATE+1)>=TRUNC(SYSDATE))
AND  (B.GDT_EFFECTIVE_FRM<=TRUNC(SYSDATE) AND NVL(B.GDT_EFFECTIVE_TO,SYSDATE+1)>=TRUNC(SYSDATE))
AND  (C.GDT_EFFECTIVE_FRM<=TRUNC(SYSDATE) AND NVL(C.GDT_EFFECTIVE_TO,SYSDATE+1)>=TRUNC(SYSDATE))
AND A.GNUM_HOSPITAL_CODE=$P{hospitalCode}
AND B.GNUM_HOSPITAL_CODE=$P{hospitalCode}
AND C.GNUM_HOSPITAL_CODE=$P{hospitalCode}
ORDER BY GSTR_DEPT_NAME,HGSTR_UNITNAME,GSTR_PATIENT_CAT_NAME]]></queryString>

