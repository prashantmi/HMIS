	<queryString><![CDATA[SELECT X.HGBSTR_ICD_GROUP_CODE AS ICDGROUPCODE, (SELECT INITCAP (HGBSTR_ICD_GROUP) FROM HGBT_ICD_GROUP_MST 
WHERE GNUM_HOSPITAL_CODE=100 AND GNUM_ISVALID=1 AND HGBSTR_ICD_GROUP_CODE=X.HGBSTR_ICD_GROUP_CODE) AS ICDGROUPFULL,
X.HGBSTR_ICD_SUBGRP_CODE AS ICDSUBGROUPCODE, INITCAP(X.HGBSTR_ICD_SUBGROUP) AS ICDSUBGROUPFULL, 
X.HGBSTR_SUBGRP_PARENT_CODE AS SUBGROUPPARENTCODE, X.HGBNUM_LEVEL AS SUBGROUPLEVEL, 
Y.HGBSTR_DISEASE_CODE AS DISEASECODE, Y.HGBSTR_DISEASE AS FULLDISEASENAME, Y.HGBSTR_PARENT_CODE AS PARENTCODE, 
(SELECT INITCAP (HGBSTR_DISEASE) FROM HGBT_ICD_DISEASE_MST WHERE GNUM_HOSPITAL_CODE=100 AND GNUM_ISVALID=1 AND HGNUM_RECORD_TYPE=0 
AND HGBSTR_PARENT_CODE IS NULL AND HGBSTR_DISEASE_CODE=Y.HGBSTR_PARENT_CODE) AS ICDPARENTDISEASENAME,
Y.HGNUM_RECORD_TYPE AS RECORDTYPE, Y.HBSTR_MAIN_DISEASE AS MAINDISEASE, Y.HGBNUM_DISEASE_TYPE_CODE AS DISEASETYPECODE, 
NVL((SELECT INITCAP(A.HGBSTR_DISEASE_TYPE_NAME) FROM HGBT_DISEASE_TYPE_MST A WHERE A.HGBNUM_DISEASE_TYPE_CODE=Y.HGBNUM_DISEASE_TYPE_CODE 
AND A.GNUM_HOSPITAL_CODE=100 AND A.GNUM_ISVALID=1),'') AS DISEASETYPENAME 

FROM HGBT_ICD_SUBGROUP_MST X, HGBT_ICD_DISEASE_MST Y
WHERE X.GNUM_HOSPITAL_CODE=100 AND X.GNUM_ISVALID=1
AND Y.GNUM_HOSPITAL_CODE=100 AND X.HGBSTR_ICD_SUBGRP_CODE=Y.HGBSTR_ICD_SUBGRP_CODE 
AND Y.GNUM_ISVALID=1 AND Y.HGNUM_RECORD_TYPE=0 AND Y.HGBSTR_PARENT_CODE IS NOT NULL 

ORDER BY X.HGBSTR_ICD_GROUP_CODE, X.HGBSTR_ICD_SUBGRP_CODE, Y.HGBSTR_PARENT_CODE, Y.HGBSTR_DISEASE_CODE]]></queryString>

