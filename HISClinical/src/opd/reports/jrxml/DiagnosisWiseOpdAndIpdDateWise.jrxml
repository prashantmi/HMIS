	</parameter>
	<queryString><![CDATA[select  nvl((select HGBSTR_DISEASE_DESC 
           from hgbt_hospital_disease_mst 
          where HGBSTR_HOSPITAL_DISEASE_CODE=a.hrgnum_diagnostic_code 
            and GNUM_HOSPITAL_CODE=100
            and gnum_isvalid=1),'---') as dignosis,
        nvl(sum(decode(opdIpd||AHIS_UTIL.PUK_TYPE(a.HRGNUM_PUK,12),'O10',1,0)),0) opd_male_adult,
        Nvl(sum(decode(opdIpd||AHIS_UTIL.PUK_TYPE(a.HRGNUM_PUK,12),'O20',1,0)),0) opd_female_adult,
        Nvl(sum(decode(opdIpd||AHIS_UTIL.PUK_TYPE(a.HRGNUM_PUK,12),'O11',1,0)),0) opd_male_child,
        Nvl(sum(decode(opdIpd||AHIS_UTIL.PUK_TYPE(a.HRGNUM_PUK,12),'O21',1,0)),0) opd_female_child,
        
        Nvl(sum(decode(opdIpd||AHIS_UTIL.PUK_TYPE(a.HRGNUM_PUK,12),'I10',1,0)),0) ipd_male_adult,
        Nvl(sum(decode(opdIpd||AHIS_UTIL.PUK_TYPE(a.HRGNUM_PUK,12),'I20',1,0)),0) ipd_female_adult,
        Nvl(sum(decode(opdIpd||AHIS_UTIL.PUK_TYPE(a.HRGNUM_PUK,12),'I11',1,0)),0) ipd_male_child,
        Nvl(sum(decode(opdIpd||AHIS_UTIL.PUK_TYPE(a.HRGNUM_PUK,12),'I21',1,0)),0) ipd_female_child,
        
        (SELECT trim(GNUM_HOSPITAL_CODE||' ') FROM gblt_hospital_mst WHERE gnum_hospital_code = a.gnum_hospital_code) AS hospitalCode,
        (SELECT gstr_hospital_name FROM gblt_hospital_mst WHERE gnum_hospital_code = a.gnum_hospital_code) AS hospitalname,
        (SELECT gstr_hospital_add1||' '|| gstr_hospital_add2||' '|| GSTR_CITY||' '|| gnum_pin_code 
           FROM gblt_hospital_mst
          WHERE gnum_hospital_code = a.gnum_hospital_code) AS hospitaladdr
        
  from  (select HRGNUM_PUK, HRGNUM_EPISODE_CODE, HRGNUM_ADMISSION_NO, HRGT_EPISODE_DATE, GNUM_HOSPITAL_CODE,
                decode(HRGNUM_ADMISSION_NO,null,'O','I') as opdIpd,hrgnum_diagnostic_code
           from hrgt_episode_diagnosis_dtl
          where gnum_isvalid = 1
            AND $P{allHospitalCode} like '%'||gnum_hospital_code||'%'
	    AND trunc(GDT_ENTRY_DATE) >= to_date($P{fromDate},'dd-Mon-yyyy') 
            AND trunc(GDT_ENTRY_DATE) <= to_date($P{toDate},'dd-Mon-yyyy')
            AND hrgnum_diagnostic_code like $P{strDiagnosisCode}) a
 where  1=1
 GROUP  BY a.gnum_hospital_code,hrgnum_diagnostic_code]]></queryString>

