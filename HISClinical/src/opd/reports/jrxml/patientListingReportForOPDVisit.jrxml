	<queryString><![CDATA[select a.HRGNUM_PUK, (select GSTR_DEPT_NAME from GBLT_DEPARTMENT_MST where GNUM_DEPT_CODE=a.GNUM_DEPT_CODE and GNUM_ISVALID=1 AND GNUM_HOSPITAL_CODE=A.GNUM_HOSPITAL_CODE
AND GDT_EFFECTIVE_FRM<=TRUNC(SYSDATE) AND NVL(GDT_EFFECTIVE_TO,SYSDATE+1)>=TRUNC(SYSDATE))dept, 
to_char(a.GDT_ENTRY_DATE,'dd-mon-yyyy')visit_date, HRGSTR_FNAME||' '||HRGSTR_MNAME||' '||HRGSTR_LNAME name, 
(select GSTR_GENDER_NAME from GBLT_GENDER_MST where GNUM_GENDER_CODE=b.GNUM_GENDER_CODE and GNUM_ISVALID=1 AND GNUM_HOSPITAL_CODE=A.GNUM_HOSPITAL_CODE)||'/'||AHIS_UTIL.dob_age_on(HRGDT_DOB,sysdate) sex_age
from HRGT_EPISODE_DTL a, HRGT_PATIENT_DTL b where a.GNUM_ISVALID=1 and b.GNUM_ISVALID=1 and a.HRGNUM_PUK=b.HRGNUM_PUK and a.GNUM_HOSPITAL_CODE=$P{hospitalCode}
and b.GNUM_HOSPITAL_CODE=$P{hospitalCode} and to_char(a.GNUM_DEPT_CODE) like $P{departmentCode} 
and a.GDT_ENTRY_DATE >= to_date($P{fromDate},'dd-mon-yyyy') and a.GDT_ENTRY_DATE <= trunc(to_date($P{toDate},'dd-mon-yyyy')+1)
order by trunc(a.GDT_ENTRY_DATE),dept]]></queryString>

