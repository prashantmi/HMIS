	<parameter name="toDepartment" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["%"]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT a.HRGNUM_PUK, 
b.HRGSTR_FNAME||' '||b.HRGSTR_MNAME||' '||b.HRGSTR_LNAME name,
(select GSTR_GENDER_SHORT from GBLT_GENDER_MST where GNUM_GENDER_CODE=b.GNUM_GENDER_CODE)||'/'||b.HRGSTR_AGE ga,
(SELECT GSTR_DEPT_NAME FROM GBLT_DEPARTMENT_MST WHERE GNUM_DEPT_CODE=A.HRGNUM_FROM_DEPT_CODE
AND GDT_EFFECTIVE_FRM<=TRUNC(SYSDATE) AND NVL(GDT_EFFECTIVE_TO,SYSDATE+1)>=TRUNC(SYSDATE) ) AS REFER_FROM, 
(SELECT GSTR_DEPT_NAME FROM GBLT_DEPARTMENT_MST WHERE GNUM_DEPT_CODE=A.HRGNUM_TO_DEPT_CODE
AND GDT_EFFECTIVE_FRM<=TRUNC(SYSDATE) AND NVL(GDT_EFFECTIVE_TO,SYSDATE+1)>=TRUNC(SYSDATE) )AS REFER_TO,
HRGDT_REFF_DATE_TIME visit_date
FROM HRGT_EPISODE_REF_DTL A,HRGT_PATIENT_DTL b
WHERE a.hrgnum_puk=b.hrgnum_puk
and (HRGSTR_IS_REFF_IN_OUT=3)
and a.HRGNUM_FROM_DEPT_CODE like($P{fromDepartment})
and a.HRGNUM_TO_DEPT_CODE like ($P{toDepartment})
AND HRGDT_REFF_DATE_TIME >=TO_DATE($P{fromDate},'dd-Mon-yyyy')
AND HRGDT_REFF_DATE_TIME <= TRUNC(TO_DATE($P{toDate},'dd-Mon-yyyy')+1)
ORDER BY HRGNUM_PUK]]></queryString>

