	<queryString><![CDATA[SELECT strICDCode, strICDName,
NVL(SUM(DECODE(strGenderChild,10,1,0)),0) AS strMale,
NVL(SUM(DECODE(strGenderChild,20,1,0)),0) AS strFemale,
NVL(SUM(DECODE(SUBSTR(strGenderChild,2,1),1,1,0)),0) AS strChildren

FROM
(SELECT HRGNUM_DIAGNOSTIC_CODE AS strICDCode, 
pkg_clinical_gbl_fun.get_icd_disease_name('0', A.GNUM_HOSPITAL_CODE, A.HRGNUM_DIAGNOSTIC_CODE) AS strICDName,
AHIS_UTIL.PUK_TYPE(B.HRGNUM_PUK,12) AS strGenderChild

FROM HRGT_EPISODE_DIAGNOSIS_DTL A, HRGT_PATIENT_DTL B
WHERE A.GNUM_ISVALID=1 AND B.GNUM_ISVALID=1
AND A.GNUM_HOSPITAL_CODE=$P{hospitalCode}

AND A.HRGNUM_VISIT_NO = (SELECT MAX(HRGNUM_VISIT_NO) FROM HRGT_EPISODE_DIAGNOSIS_DTL 
WHERE GNUM_ISVALID=1 AND HRGNUM_PUK=A.HRGNUM_PUK AND HRGNUM_EPISODE_CODE=A.HRGNUM_EPISODE_CODE 
AND GNUM_HOSPITAL_CODE=A.GNUM_HOSPITAL_CODE) 
AND A.HBNUM_ISREPEAT IN (0,1) 
AND TRUNC(A.HRGT_EPISODE_DATE) >= TRUNC(TO_DATE($P{fromDate},'dd-Mon-yyyy'))
AND TRUNC(A.HRGT_EPISODE_DATE) <= TRUNC(TO_DATE($P{toDate},'dd-Mon-yyyy'))
AND A.GNUM_HOSPITAL_CODE=B.GNUM_HOSPITAL_CODE
AND A.HRGNUM_PUK=B.HRGNUM_PUK)

GROUP BY strICDCode, strICDName
ORDER BY strICDCode]]></queryString>

