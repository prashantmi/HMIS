		<defaultValueExpression ><![CDATA["GTB"]]></defaultValueExpression>
	</parameter>
	<parameter name="departmentCode" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["103"]]></defaultValueExpression>
	</parameter>
	<parameter name="unit" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["%"]]></defaultValueExpression>
	</parameter>
	<parameter name="hospitalCode" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["108"]]></defaultValueExpression>
	</parameter>
	<parameter name="icdCode" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["%"]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[Select (select GSTR_DEPT_NAME From GBLT_DEPARTMENT_MST Where GNUM_DEPT_CODE=x.GNUM_DEPT_CODE and GNUM_ISVALID=1 AND gnum_hospital_code = $P{hospitalCode}) dept_name,
(select HGSTR_UNITNAME from HGBT_UNIT_MST where HGNUM_DEPTUNITCODE=x.HGNUM_DEPTUNITCODE and GNUM_ISVALID=1 AND gnum_hospital_code =$P{hospitalCode}) unit_name,
x.HRGNUM_DIAGNOSTIC_CODE,
x.N_M,x.N_F,x.N_M_C,x.N_F_C,

x.O_M,x.O_F,x.O_M_C,x.O_F_C

from
(SELECT GNUM_DEPT_CODE,HGNUM_DEPTUNITCODE,HRGNUM_DIAGNOSTIC_CODE,

Nvl(sum(decode(HRGNUM_IS_OLD,0,DECODE(AHIS_UTIL.PUK_TYPE(a.HRGNUM_PUK,12),10,1,0),0)),0) N_M,

Nvl(sum(decode(HRGNUM_IS_OLD,0,DECODE(AHIS_UTIL.PUK_TYPE(a.HRGNUM_PUK,12),20,1,0),0)),0) N_F,

Nvl(sum(decode(HRGNUM_IS_OLD,0,DECODE(AHIS_UTIL.PUK_TYPE(a.HRGNUM_PUK,12),11,1,0),0)),0) N_M_C,

Nvl(sum(decode(HRGNUM_IS_OLD,0,DECODE(AHIS_UTIL.PUK_TYPE(a.HRGNUM_PUK,12),21,1,0),0)),0) N_F_C,

Nvl(sum(decode(HRGNUM_IS_OLD,1,DECODE(AHIS_UTIL.PUK_TYPE(a.HRGNUM_PUK,12),10,1,0),0)),0) O_M,

Nvl(sum(decode(HRGNUM_IS_OLD,1,DECODE(AHIS_UTIL.PUK_TYPE(a.HRGNUM_PUK,12),20,1,0),0)),0) O_F,

Nvl(sum(decode(HRGNUM_IS_OLD,1,DECODE(AHIS_UTIL.PUK_TYPE(a.HRGNUM_PUK,12),11,1,0),0)),0) O_M_C,

Nvl(sum(decode(HRGNUM_IS_OLD,1,DECODE(AHIS_UTIL.PUK_TYPE(a.HRGNUM_PUK,12),21,1,0),0)),0) O_F_C


From HRGT_EPISODE_DTL a,HRGT_EPISODE_DIAGNOSIS_DTL b 

WHERE A.GNUM_DEPT_CODE LIKE ($P{departmentCode})
and a.HRGNUM_PUK=b.HRGNUM_PUK 
and a.HRGNUM_EPISODE_CODE=b.HRGNUM_EPISODE_CODE
and a.HRGNUM_VISIT_NO=b.HRGNUM_VISIT_NO
AND (A.HGNUM_DEPTUNITCODE LIKE ($P{unit}) and a.GNUM_DEPT_CODE like ($P{departmentCode}))
and b.HRGNUM_DIAGNOSTIC_CODE LIKE ($P{icdCode})
and A.GNUM_ISVALID=1
and a.gnum_hospital_code=$P{hospitalCode}
and b.gnum_hospital_code=$P{hospitalCode}
AND HRGDT_EPISODE_DATE >= TO_DATE($P{fromDate},'dd-Mon-yyyy')
AND HRGDT_EPISODE_DATE<= TRUNC(TO_DATE($P{toDate},'dd-Mon-yyyy')+1)
group by GNUM_DEPT_CODE,HGNUM_DEPTUNITCODE,HRGNUM_DIAGNOSTIC_CODE)x
order by unit_name]]></queryString>

