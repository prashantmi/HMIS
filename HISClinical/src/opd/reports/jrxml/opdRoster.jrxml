	<queryString><![CDATA[/* Formatted on 2011/09/06 11:32 (Formatter Plus v4.8.8) */
SELECT   (   (SELECT gstr_dept_name
                FROM gblt_department_mst
               WHERE gnum_dept_code = SUBSTR (a.hgnum_deptunitcode, 1, 3)
                 AND gnum_hospital_code = a.gnum_hospital_code
                 AND gnum_isvalid = 1)
          || '/'
          || hgstr_unitname
         ) AS dept_unitname,
         unitdays (a.hgnum_deptunitcode, a.gnum_hospital_code) dayandtime,
         unitrooms (a.hgnum_deptunitcode, a.gnum_hospital_code) AS room,
         (SELECT pkg_bloodbank_mst.emp_name_empidwise
                                                (a.gnum_hospital_code,
                                                 hgstr_emp_code
                                                )
            FROM hgbt_unit_consultant_mst
           WHERE gnum_hospital_code = a.gnum_hospital_code
             AND gnum_isvalid = 1
             AND hgnum_is_hou = 1
             AND hgnum_deptunitcode = a.hgnum_deptunitcode) AS hou
    FROM hgbt_unit_mst a
   WHERE a.gnum_hospital_code = $P{allHospitalCode}
   AND a.gnum_isvalid = 1
--and a.hgnum_deptunitcode=24811
ORDER BY dept_unitname]]></queryString>

