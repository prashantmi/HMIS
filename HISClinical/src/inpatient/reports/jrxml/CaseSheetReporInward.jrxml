	<queryString><![CDATA[SELECT a.hrgnum_puk AS patcrno,
       TO_CHAR(d.hipdt_admdatetime,'dd-Mon-yyyy')  admdatetime,
     INITCAP(c.HRGSTR_FNAME)||' '||INITCAP(c.HRGSTR_MNAME)||' '||INITCAP(c.HRGSTR_LNAME) AS patname,
	   DECODE(a.HIPNUM_CASESHEET_STATUS,1,'In Ward',4,'Lost') status,(SELECT gstr_gender_short FROM GBLT_GENDER_MST
	     WHERE gnum_gender_code = c.GNUM_GENDER_CODE AND  gnum_hospital_code = c.gnum_hospital_code)||'( '||
Ahis_Util.puk_age_on (a.hrgnum_puk, SYSDATE)||')' AS patage,DECODE(a.HIPNUM_IS_DUPLICATE,0,'Original',1,'Duplicate') duplicate,
(SELECT hgstr_unitname FROM HGBT_UNIT_MST WHERE hgnum_deptunitcode = a.gnum_deptunit_code AND gnum_hospital_code = a.gnum_hospital_code AND gnum_isvalid=1  AND TRUNC (gdt_effective_frm) <= TRUNC (SYSDATE) 
AND TRUNC (NVL (gdt_effective_to, SYSDATE)) >= TRUNC (SYSDATE))||'('||(SELECT hipstr_ward_name FROM HIPT_WARD_MST WHERE hipnum_ward_code = a.hipnum_ward_code 
AND gnum_hospital_code = a.gnum_hospital_code AND gnum_isvalid = a.GNUM_ISVALID
AND gdt_effective_frm <= SYSDATE AND (TO_DATE (NVL (gdt_effective_to, SYSDATE), 'DD-MM-YY') >=TO_DATE (SYSDATE, 'DD-MM-YY')))||')' ward 
FROM HIPD_CASESHEET_DTL a,
       HRGT_PATIENT_DTL c,HIPT_PATADMDISC_DTL d
 WHERE a.hrgnum_puk = c.HRGNUM_PUK 
   AND a.gnum_deptunit_code LIKE $P{unitCode}
   AND a.hipnum_ward_code LIKE $P{wardCode}
   AND a.gnum_isvalid = 1
   AND a.gnum_hospital_code = $P{hospitalCode}
   AND a.gnum_hospital_code = c.gnum_hospital_code AND a.GNUM_HOSPITAL_CODE=d.GNUM_HOSPITAL_CODE
   AND d.HRGNUM_PUK=a.HRGNUM_PUK
   AND a.gnum_isvalid = c.gnum_isvalid AND a.GNUM_ISVALID=d.GNUM_ISVALID AND a.HIPNUM_CASESHEET_STATUS IN (1)]]></queryString>

