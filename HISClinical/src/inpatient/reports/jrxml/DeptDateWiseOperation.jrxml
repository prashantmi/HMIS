	<queryString><![CDATA[/* Formatted on 2009/11/16 17:41 (Formatter Plus v4.8.7) */
SELECT   NVL ((SELECT   COUNT (a.hotnum_ot_req_no)
                   FROM HOTT_OP_RAISED_DTL a,
                        HOTT_OPERATION_MST e,
                        HOTT_OP_REQUEST_DTL c
                  WHERE a.hotnum_ot_req_no = c.hotnum_ot_req_no
                    AND a.gnum_hospital_code = v.gnum_hospital_code
                    AND a.gnum_isvalid = 1
                    AND SUBSTR (a.hotnum_prop_deptop_code, 1, 3) =
                                                              c.gnum_dept_code AND
                        c.gnum_dept_code=v.GNUM_DEPT_CODE AND c.HGNUM_DEPTUNITCODE=u.HGNUM_DEPTUNITCODE
                    AND e.hotnum_operation_code =
                                         SUBSTR (a.hotnum_prop_deptop_code, 4)
                    AND e.hotnum_operation_type_code LIKE '10'
                    AND c.gnum_hospital_code = v.gnum_hospital_code
                    AND c.gnum_isvalid = 1
                    AND c.hotnum_patient_acceptance IN (1, 2)
                    AND (   c.hotstr_ot_req_status > 12
                         OR c.hotstr_ot_req_status = 9
                        )
                    AND TRUNC (c.hotdt_operation_date) >=
                                 TRUNC (TO_DATE ('4-aug-2007', 'dd-Mon-yyyy'))
                    AND TRUNC (c.hotdt_operation_date) <=
                              TRUNC (TO_DATE ('4-dec-2009', 'dd-Mon-yyyy'))
                   
               GROUP BY e.hotnum_operation_type_code),
              0
             ) countmajor,
             
         NVL ((SELECT   COUNT (a.hotnum_ot_req_no)
                   FROM HOTT_OP_RAISED_DTL a,
                        HOTT_OPERATION_MST e,
                        HOTT_OP_REQUEST_DTL c
                  WHERE a.hotnum_ot_req_no = c.hotnum_ot_req_no
                    AND a.gnum_hospital_code = v.gnum_hospital_code
                    AND a.gnum_isvalid = 1
                    AND c.gnum_dept_code=v.GNUM_DEPT_CODE
                    AND c.HGNUM_DEPTUNITCODE=u.HGNUM_DEPTUNITCODE
                    AND SUBSTR (a.hotnum_prop_deptop_code, 1, 3) =
                                                              c.gnum_dept_code
                    AND e.hotnum_operation_code =
                                         SUBSTR (a.hotnum_prop_deptop_code, 4)
                    AND e.hotnum_operation_type_code LIKE '11'
                    AND c.gnum_hospital_code = v.gnum_hospital_code
                    AND c.gnum_isvalid = 1
                    AND c.hotnum_patient_acceptance IN (1, 2)
                    AND (   c.hotstr_ot_req_status > 12
                         OR c.hotstr_ot_req_status = 9
                        )
                    AND TRUNC (c.hotdt_operation_date) >=
                                 TRUNC (TO_DATE ($P{fromDate}, 'dd-Mon-yyyy'))
                    AND TRUNC (c.hotdt_operation_date) <=
                              TRUNC (TO_DATE ($P{toDate}, 'dd-Mon-yyyy'))
                   
               GROUP BY e.hotnum_operation_type_code, c.hgnum_deptunitcode),
              0
             ) countminor,
         v.gstr_dept_name || '-' || u.hgstr_unitname unit,
         (SELECT trim(GNUM_HOSPITAL_CODE||' ') FROM gblt_hospital_mst WHERE gnum_hospital_code = u.gnum_hospital_code) AS hospitalCode,
        (SELECT gstr_hospital_name FROM gblt_hospital_mst WHERE gnum_hospital_code = u.gnum_hospital_code) AS hospitalname,
        (SELECT gstr_hospital_add1||' '|| gstr_hospital_add2||' '|| GSTR_CITY||' '|| gnum_pin_code 
           FROM gblt_hospital_mst
          WHERE gnum_hospital_code = u.gnum_hospital_code) AS hospitaladdr
    FROM GBLT_DEPARTMENT_MST v, HGBT_UNIT_MST u
   WHERE v.gnum_hospital_code = $P{hospitalCode}
     AND v.gnum_isvalid = 1
     AND v.gnum_dept_code = u.hgnum_dept_code
     AND v.gdt_effective_frm <= TRUNC (SYSDATE)
     AND NVL (v.gdt_effective_to, SYSDATE + 1) >= TRUNC (SYSDATE)
     AND u.gnum_hospital_code = v.gnum_hospital_code
     AND u.gnum_isvalid = 1
     AND u.hgnum_isgeneral LIKE $P{speciality} AND u.HGNUM_DEPTUNITCODE LIKE $P{unit}
     AND u.gdt_effective_frm <= TRUNC (SYSDATE)
     AND NVL (u.gdt_effective_to, SYSDATE + 1) >= TRUNC (SYSDATE)
ORDER BY unit]]></queryString>

