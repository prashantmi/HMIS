	<queryString><![CDATA[SELECT  INITCAP (c.hrgstr_fname)|| ' '|| INITCAP (c.hrgstr_mname)|| ' '|| INITCAP (c.hrgstr_lname) AS patname,
a.HRGNUM_PUK patcrno,a.HANCNUM_GRAVIDA_NO gravida,a.HANCNUM_REGISTRATION_AGE age,a.HANCNUM_LIVEBIRTH_COUNT children,
DECODE(a.HANCNUM_IS_BROUGHTBY_ASHA,NULL,'-',a.HANCNUM_IS_BROUGHTBY_ASHA,(SELECT b.HANCSTR_HEALTH_WORKER FROM
 HANCT_HEALTH_WORKER_MST b
WHERE a.HANCNUM_HEALTH_WORKER_ID=b.HANCNUM_HEALTH_WORKER_ID AND a.GNUM_HOSPITAL_CODE=b.GNUM_HOSPITAL_CODE AND
a.GNUM_ISVALID=b.GNUM_ISVALID))broughtby,TO_CHAR(a.GDT_ENTRY_DATE,'DD-Mon-YYYY')regDate,TO_CHAR(a.GDT_ENTRY_DATE,'DD-Mon-YYYY')GRP_DATE
 FROM HANCT_JSY_REGISTRATION_DTL a, HRGT_PATIENT_DTL c WHERE a.GNUM_ISVALID=1 AND a.GNUM_HOSPITAL_CODE=$P{hospitalCode}
 AND a.HRGNUM_PUK=c.HRGNUM_PUK AND    TRUNC(a.GDT_ENTRY_DATE)>= TRUNC(TO_DATE($P{fromDate},'dd-mon-yyyy'))
AND TRUNC(a.GDT_ENTRY_DATE) <= TRUNC(TO_DATE($P{toDate},'dd-mon-yyyy')) order by a.GDT_ENTRY_DATE]]></queryString>

